       <div class="clearfix filtros"> 
        <script type="text/javascript">
    $(window).load(function(){
        getUserLocation(setLocalizacaoForm,function(){});
    });
    
    var sliderTimeout;
    function restFloat(num) {
        return num.toString().replace(".","p").replace(",","p");
    }
    function setSliderValue(campos,comp) {
        if(sliderTimeout) {
            clearTimeout(sliderTimeout);
        }
        var valores='';
        if(comp.realvalues!=undefined)
            valores=restFloat(comp.realvalues[0])+"_"+restFloat(comp.realvalues[1]);
        else if(comp.realvalue!=undefined)
            valores=restFloat(comp.realvalue);
        sliderTimeout=setTimeout("b_AFDirect('"+campos+"','"+valores+"')",500);
    }

    function setVersaoAutoComplete(id) {
        var parts = id.split('___');
        if(parts.length > 2){
            id = id.substring(0, id.lastIndexOf("___"));
        }
        if(id.indexOf('mar') != -1){
            registrarEventoFiltro('Adicionar Filtro', 'Marca');
        } else if(id.indexOf('mod') != -1){
            registrarEventoFiltro('Adicionar Filtro', 'Modelo');
        } else if(id.indexOf('ver') != -1){
            registrarEventoFiltro('Adicionar Filtro', 'Versão');
        }
        b_AFSOP(id);
    }

    function setLocalizacaoAutoCompleteBusca(id, event) {
        var source = $(event.srcElement);
        try{$('.icc_ftr_btn_lst').removeClass('ftr_btn_lst_open');} catch(e) {}
        params=id.split("___");
        if(source.attr('id') == 'novalocalizacao'){
            if(params[0]=="cep") {
                b_AFDirect("geo_esc_rai",params[1]+ "|0|0_2_5");
            } else {
                ciduf=params[1].split("-");
                b_AFDirect("est_cid_esc_geo",ciduf[1]+"_"+ciduf[0]+"_1_0|0|0");
            }
        } else {
            if (id.length > 0) {
                var params = id.split("_");
                if (params[0] == "cep") {
                    try {
                        data={};
                        data.texto='até 25km de distância do CEP ' + params[3];
                        data.sop='geo_'+params[3]+'|0|0.1_-esc_2.1_-rai_25.1_';
                        setLocalizacaoForm(data);
                    } catch (e) {console.log(e);}
                } else {
                    try {
                        var ciduf = params[3].split("-");
                        data={};
                        data.texto= params[params.length - 1];
                        data.sop='cid_'+ciduf[0]+'.1_-est_'+ciduf[1]+".1_-esc_2.1_-rai_25.1_"
                        setLocalizacaoForm(data);
                    } catch (e) {console.log(e);}
                }
            }
        }
    }

    function setLocalizacaoForm(data) {
        var regex = /esc_([0-9])/g;
        var match = regex.exec(data.sop);
        if(match != null && match.length > 1){
            switch (match[1]){
                case 2 : {
                    $('#localizacaoCidadeFiltro').attr('checked', 'checked');
                    break;
                }
                case 3: {
                    $('#localizacaoCidadeFiltro').attr('checked', 'checked');
                    break;
                }
                case 4:
                    $('#localizacaoCidadeFiltro').attr('checked', 'checked');
                    break;
                default :
                    $('#localizacaoCidadeFiltro').attr('checked', 'checked');
            }
        }
        $('#buscaFormFiltro input[name="locationSop"]').remove();
        $('#buscaFormFiltro').append('<input type="hidden" name="locationSop" value="' + data.sop + '" />');
        $('#cidadeAbertoTextoFiltro').text(data.texto);
        $('#buscaFormFiltro').find('.toggle_location').toggle();
    }

    function setLocalizacaoAutoComplete(id) {
        try{$('.icc_ftr_btn_lst').removeClass('ftr_btn_lst_open');} catch(e) {}
        params=id.split("___");
        if(params[0]=="cep") {
            b_AFDirect("geo_esc_rai",params[1]+ "|0|0_2_5");
        } else {
            ciduf=params[1].split("-");
            b_AFDirect("est_cid_esc_geo",ciduf[1]+"_"+ciduf[0]+"_1_0|0|0");
        }
    }

    function setToLocalizacaoAtual() {
        $('.localizacao_atual').toggle();
        getDeviceLocation(function (data){
            setLocalizacaoForm(data);
            $('.localizacao_atual').toggle();
        }, function(){})
    }

    

</script> 
