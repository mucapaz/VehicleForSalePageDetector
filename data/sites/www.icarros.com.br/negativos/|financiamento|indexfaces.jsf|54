            /* <![CDATA[ */

            $(document).ready(function(){
                $('#simuladorForm .tab-container').icarrosToggleTabs({
                    onChange : function(){
                        var form = $('#simuladorForm');
                        form.find('input[type=hidden][name="financNumParcelas"]').val($(event.srcElement).attr('for'));
                    }
                });
                $('#btnFinanciar').click(function(){
                    return validarFormSimulador($('#simuladorForm'));
                });
                $(".titulo_duvidas").click(function(){
                    $("#textos_duvidas").show();
                });
                $('input[data-fmt]').each(function(){
                    var fmtType = $(this).attr('data-fmt').toLowerCase();
                    formatCamp($(this)[0], fmtType);
                });
            });

