<div class="form__area" id="form_detalhes" onchange="VrumJs.itensChange('detalhes');return false;">
    <div class="row vehicle__details">
        {% for (var i=0; i<o.length; i++) { %}
            {% for (var item in o[i]) { %}
                {% if (o[i][item]) { %}
                    <fieldset class="col-sm-4">
                        <label for="{%=o[i][item].nome%}">
                            <span>{%=o[i][item].title%} {% if (o[i][item].require) { %}*{% } %}</span>
                            {% if (o[i][item].type =='text') { %}
                                <input type="text" name="{%=o[i][item].nome%}" id="{%=o[i][item].id%}" placeholder="{%=o[i][item].placeholder%}"
                                {% if (o[i][item].tig_id != '') { %}
                                    gen_tig="{%=o[i][item].tig_id%}"
                                {% } %}
                                >
                            {% } else { %}
                                <select name="{%=o[i][item].nome%}" id="{%=o[i][item].id%}"
                                {% if (o[i][item].tig_id != '') { %}
                                    gen_tig="{%=o[i][item].tig_id%}"
                                {% } %}
                                >
                                    <option value="">{%=o[i][item].placeholder%}</option>
                                </select>
                            {% } %}
                        </label>
                    </fieldset>
                {% } %}
            {% } %}
        {% } %}
    </div>
</div>
</script> 
