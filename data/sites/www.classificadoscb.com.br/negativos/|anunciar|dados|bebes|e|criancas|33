    <script type="text/x-tmpl" id="premium">
	<div class="plan__sections tab-pane fade active" id="planos">
		<div class="row">
			{% for (var i=0; i<o.length; i++) { %}
				<div class="col-lg-3 col-md-6 col-sm-12 {% if( (o.length==2) && (i == 0) ) { %} col-lg-offset-3 {% } %} {% if( (o.length==3) && (i == 0) ) { %} col-lg-offset-1_5 {% } %}">
					<div class="plan__bx__content {% if(o[i].prioridade=='2'){ %} recomendado {% } %}" data-type-plan="{%=o[i].title%}">
						<div class="plan__header">
							<span class="plan__name">
								{%=o[i].title%}
								{% if(o[i].prioridade=='2'){ %}
									<span>(Recomendado)</span>
								{% } %}
							</span>
							{% if (o[i].preco =='0.00') { %}
								<span class="plan__value">Gr&aacute;tis</span>
							{% } else { %}
								<span class="plan__value">R$ {%=o[i].preco%}</span>
							{% } %}
						</div>
						<div class="plan__description">
							<ul>
								{% for (var x=0; x<o[i].detalhes.length; x++) { %}
									<li>{%#o[i].detalhes[x] %}</li>
								{% } %}
							</ul>
							<a href="javascript:planoClique({%=o[i].ppc_pav_id%})"
							class="btn btn-plan {% if(o[i].prioridade=='2'){ %} btn-recomendado {% } %}">
							Anuncie agora
							</a>
						</div>
