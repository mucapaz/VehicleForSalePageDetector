			window.secao = {};

			var idanuncio = "False";
			var fotos = [];
			if ($("*[data-step='1']").length !== 0 && idanuncio == "False") {
				$('#plan__select').modal({
					show: true
				});
			}else{
				jQuery('a.js-subcategoria').each(function(e, val){
					if ($(val).attr('data-ppc_cat_id') == $('#ppc_atr_id_20002').val()){
						$(val).click();
						return false;
					}
				})
			}

			if (fotos){
				var contentMain = $('#photo__upload').closest(".media__bx"),
					contentBx = contentMain.find(".media__bx__content");

				for (foto in fotos){
					var template = contentMain.find('.template__model').clone().removeClass('template__model');
					template.find(".media__thumb__image").append("<img src='"+ fotos[foto].url_temp + "/"+ fotos[foto].nome +"' />");

					template.find(".js-delete").on("click", function() {
						UpFotoJs.deleteImg(jQuery(this));
					});

					contentBx.append(template);
					$('.media__thumb').not('.template__model').show();
					if (Modernizr.flexboxlegacy &&  Modernizr.flexbox){
						jQuery('.media__thumb__image').css({
							'display':'flex'
						});
					}
					UpFotoJs.ajustaOrder();
				}
			}

		});

		jQuery('a.js-subcategoria').click(function(){
			var self = jQuery(this);
			renderPlanos(self);
			var ppc_cat_id = self.data('ppc_cat_id');
			var gen_tig_idsecao = self.data('gen_tig_idsecao');
			jQuery('#ppc_atr_id_20002').val(ppc_cat_id);
			if(gen_tig_idsecao in window.secao){
				jQuery('#ppc_atr_id_20004 option').remove();
				jQuery('#ppc_atr_id_20004').append(
					jQuery('<option selected="selected" value="">selecione uma seção</option>')
				)
				jQuery(window.secao[gen_tig_idsecao]['item']).each(function(item, value){
					jQuery('#ppc_atr_id_20004').append(
						jQuery('<option value="' + value['gen_tip_valor'] + '">' + value['gen_tip_nome'] + '</option>')
					)
				});
				///jQuery('#ppc_atr_id_20004').attr('required', 'required');
				jQuery('#ppc_atr_id_20004').attr({
					'da-validator': 'true',
					'da-validator-ischecked': 'true',
					'data-da-validator-ischecked-msg': 'Selecione um seção'
				});
				var secaofield = "";
				jQuery('#ppc_atr_id_20004').val(secaofield);
				//da-validator da-validator-ischecked="true" data-da-validator-ischecked-msg="Selecione um seção"
				jQuery('#fieldsetsecao').show();
			} else {
				jQuery('#ppc_atr_id_20004 option').remove();
				jQuery('#ppc_atr_id_20004').removeAttr( 'da-validator-ismoney data-da-validator-ismoney-msg da-validator da-validator-ischecked data-da-validator-ischecked-msg' );
				// jQuery('#ppc_atr_id_20004').removeAttr('required');
				jQuery('#fieldsetsecao').hide();
			}

			//ajusta labels dos inputs de acordo com a subcategoria selecionada
			deParaSubcatLabelSecao = {'default':'Seção', '8001':'Raça'};
			if (ppc_cat_id in deParaSubcatLabelSecao){
				jQuery('#label_secao').html(deParaSubcatLabelSecao[ppc_cat_id] );
			} else{
				jQuery('#label_secao').html(deParaSubcatLabelSecao['default'] );
			}

			deParaSubcatLabelPreco = {'default':'Preço do produto', '16001':'Salário', '14002':'Preço', '11001':'Preço', '11002':'Preço', '11003':'Preço', '11004':'Preço', '15001':'Preço', '15002':'Preço', '15003':'Preço', '15004':'Preço', '15005':'Preço', '15006':'Preço', '15007':'Preço', '15008':'Preço', '15009':'Preço', '8001': 'Preço'};
			if (ppc_cat_id in deParaSubcatLabelPreco){
				jQuery('#label_preco').html(deParaSubcatLabelPreco[ppc_cat_id]);
			} else{
				jQuery('#label_preco').html(deParaSubcatLabelPreco['default']);
			}

			if (ppc_cat_id in {
					'16002': '', '16003': '', '16005': '', '16006': '',
					'16007': '', '16001': '', '11001': '', '11002':'',
					'11003':'', '11004':'', '14002': '','15001': '',
					'15002': '', '15003': '', '15004': '',
					'15005': '', '15006': '', '15007': '',
					'15008': '', '15009': ''}) {
				jQuery('#preco').removeAttr( 'da-validator-ismoney data-da-validator-ismoney-msg da-validator da-validator-ischecked data-da-validator-ischecked-msg' );
			}
			if (ppc_cat_id in {'16002': '', '16003': '', '16005': '', '16006': '', '16007': ''}){
				jQuery('#preco').closest('fieldset').hide();
			}else{
				jQuery('#preco').closest('fieldset').show();
			}

			var phoness = Array.prototype.slice.call( jQuery('.js-fone') );
			phoness.forEach(function(el, i) {
				el.addEventListener('keyup', function(){
					var phone = el.value,
						digits = phone.replace(/\D/g, '');

					VMasker(el).unMask();
					if(digits.length > 10){
						VMasker(el).maskPattern("(99) 99999-9999");
					}else{
						VMasker(el).maskPattern("(99) 9999-9999");
					}
				});
			});

		});

		jQuery( '.js-pass-step' ).on( 'click' , function(e){
			e.preventDefault();

			validator.init();

			if( validator.totalItemInvalid() === 0 ) {
				$( '.js-data' ).submit();
				$( '.js-alert' ).remove();
			}
		});

		planoClique=function(id){
			jQuery('#ppc_pav_id').val(id);
			jQuery('#plan__select').modal('hide');
		};
	</script>   
