   <script type="text/javascript">
$J(document).ready(function () {
    $J( '#login' ).submit( function (e) {
        var error;
        if ( $J( '#email' ).val().length < 6 ) {
            error = __( 'login_error_validate_email' );
            $J( '#email_error' ).show().html( error );
            $J( '#email' ).addClass( 'error' );
        }
        if ( $J( '#password' ).val().length < 3 ) {
            error = __( 'login_error_validate_pass' );
            $J( '#password_error' ).show().html( error );
            $J( '#password' ).addClass( 'error' );
        }

        if (error) {
            e.preventDefault();
        }
    } );
});
</script> 
