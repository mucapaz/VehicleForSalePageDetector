   <script type="text/javascript">
$J(function() {
    if( typeof App == 'undefined' ) { //TODO remove these when we convert to appjs
        var w = window, $ = window.jQuery;
        var ie7 = ( navigator.appName == 'Microsoft Internet Explorer' &&
                    navigator.appVersion.search( 'MSIE 7' ) > 0 );
        $subcat_counter_flag = true;
        if ($J('#vs-subcat-dropdown').children().first().children().length == 0) {
            $subcat_counter_flag = false;
        }

        //#5692 
        //adding styling with javascript and setting href on click for breadcrumb elements with links
        $J( '.mmmenu .tab' ).has('a').css( 'cursor', 'pointer' ).click( function(){
            window.location.href = $J( this ).children('a').attr('href');
            return false;
        } );
        
        $home_counter_flag = true;
        if ($J('#vs-home-dropdown').children().first().children().length == 0) {
            $home_counter_flag = false;
        }

        //#5692 - if the home geo link is present, have the dropdown come from that breadcrumb
        var $home_link = $J("#vs_bc_home_link");
        var left_adjust = 3;
        if( $J("#vs_bc_geo_link").length > 0 ) {
             $home_link = $J("#vs_bc_geo_link");
                          left_adjust = 3;
                     }
        
        if ($home_counter_flag) {
            $home_link.mouseover(function() {
                $J(this).addClass("hover");
                $J("#vs-home-dropdown").addClass("hover").css('left', $home_link.position().left - left_adjust );
                if( ie7 ) {
                    $J("#vs-home-dropdown").css( 'top', '82px' );
                }
            });
        }

        $J("#vs-home-dropdown").mouseover(function() {
            $J(this).addClass('hover ondropdown').css('left', $home_link.position().left - left_adjust );
        });

        if ($home_counter_flag) {
            $home_link.mouseout(function() {
                $J(this).removeClass("hover");
                setTimeout(function() {
                    if (!$J("#vs-home-dropdown").hasClass("ondropdown") && !$home_link.hasClass('hover')) {
                        $J("#vs-home-dropdown").removeClass("hover");
                    }
                }, 100)

            });
        }

        $J("#vs-home-dropdown").mouseout(function() {
            $J(this).removeClass('hover ondropdown');
        });
        
        var toggleHover = function( el, add ) {
            var level = $J(el).attr( 'level' );
            
            var $el = $J( '.mmmenu li[level=' + level + ']' );
            if( !$el.hasClass( 'last' ) ) {
                var $prev = $J( '.mmmenu li[level=' + ( level - 1 ) + ']' );
                if( add ) {
                    $el.addClass( 'hover' );
                    $prev.addClass( 'next-hover' );
                }
                else {
                    $el.removeClass( 'hover' );
                    $prev.removeClass( 'next-hover' );
                }
            }
        };
        
        $J( '.mmmenu li.tab' ).mouseenter( function() {
            toggleHover( this, true );
        } ).mouseleave( function() {
            toggleHover( this, false );
        } );
    }
});
</script> 
