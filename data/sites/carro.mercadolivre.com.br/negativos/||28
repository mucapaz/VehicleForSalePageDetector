        (function preparePromoSlider(){

            var autoplay = "true";

            var isAnimationEnded = false;

            var handleAnimationend = function() {
                isAnimationEnded = true;
                initSlider(autoplay);
            }

            var forceAnimationend = function() {
                handleAnimationend();
            }

            window.setTimeout(forceAnimationend, 2000);

            var $bannerItems = $('.banner-items');

            $bannerItems.on('animationend', handleAnimationend);

            function initSlider(isAutoplay) {

                $('.animated').removeClass('animated'); // impide correr nuevamente la animacion luego de armado el slider

                var promoSlider = $("#convertionSlider").carousel({
                    maxItems: 1,
                    arrows: "over",
                    pagination: true
                });

                if (isAutoplay) {
                    // auto rotador
                    promoSlider.play();

                    // pausa rotador al finalizar la primera iteracion
                    promoSlider.on('select', function() {
                        if (promoSlider.page() == 1) {
                            promoSlider.pause();
                        }
                    });

                    // pausa rotador en click sobre flechas, circulos y banner
                    $(promoSlider.element).on('click', function() {
                        promoSlider.pause();
                    });
                }

                // Removing mask provisory
                $("#convertionSlider ul").removeClass('carousel-mask');

                var animation = new home.Animation();
                animation.init();

            }
        }());

