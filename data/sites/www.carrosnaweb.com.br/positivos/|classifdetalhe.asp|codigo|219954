	function MM_openBrWindow(theURL,winName,features) 
	{ 
		window.open(theURL,winName,features);
	}
	function MM_openUnWindow(theURL,winName,features) 
	{ 
		window.open(theURL,winName,features).blur();
	}
	function Num(tecla)
	{
		if ((tecla<47)||(tecla>57))
		{
			return false
		}
		else
		{
			return true
		}
	}
	function ContaTexto(CampoMsg,CampoContador,TamMax)
	{
		if(CampoMsg.value.length>TamMax)
			CampoMsg.value=CampoMsg.value.substring(0,TamMax);
		else
			CampoContador.value=TamMax-CampoMsg.value.length;
	}			
	function setafoco() { 
		document.form2.nome.focus();
	}	
function checa()
{
	var error_string = "";
	var PrimeiroErro = "";
	if (window.document.form2.descricao.value =="")
	{
		error_string += "Favor preencher a proposta.\n";
		PrimeiroErro = "descricao";
	}
	if (window.document.form2.telefone.value =="")
	{
		error_string += "Favor preencher o seu telefone.\n";
		PrimeiroErro = "telefone";
	}
	else
	if(window.document.form2.telefone.value<100000)
	{
		error_string += "Telefone inv�lido.\n";
		PrimeiroErro = "telefone";
	}
	if (window.document.form2.ddd.value =="")
	{
		error_string += "Favor preencher o seu DDD.\n";
		PrimeiroErro = "ddd";
	}
	else
	if(window.document.form2.ddd.value<10)
	{
		error_string += "DDD inv�lido.\n";
		PrimeiroErro = "ddd";
	}
	if (window.document.form2.email.value =="")
	{
		error_string += "Favor preencher o seu e-mail.\n";
		PrimeiroErro = "email";
	}
	else
	{
		var AtSym  = document.form2.email.value.indexOf('@')
		var Period = document.form2.email.value.lastIndexOf('.')
		var Space  = document.form2.email.value.indexOf(' ')
		var Length = document.form2.email.value.length - 1
		if ( (AtSym < 1) ||       // '@' cannot be in first position
		   (Period <= AtSym+1) || // Must be atleast one valid char btwn '@' and '.'
		   (Period == Length ) || // Must be atleast one valid char after '.'
		   (Space  != -1) )       // No empty spaces permitted
		{  
			error_string += "Formato de e-mail incorreto.\n";
			PrimeiroErro = "email";
		}
	}
	if (window.document.form2.nome.value =="")
	{
		error_string += "Favor preencher o seu nome.\n";
		PrimeiroErro = "nome";
	}
	if (error_string == "")
	{
		return true;
	}
	else
	{
		alert(error_string);
		RecebeErro = 'document.form2.'+PrimeiroErro+'.focus();';
		eval(RecebeErro);
		return false;
	}	
}
