       <section class="main-section item-description  item-description__html"> 
        <h2 class="main-section__title item-description__title">DescriÃ§Ã£o do vendedor</h2> 
        <div class="item-description__content "> 
         <script>
		(function(exports, doc) {
			function removeLoadingClass() {
				var description = document.querySelector('.item-description');
				if (exports.tiny) {
					tiny.removeClass(description, 'is-loading');
					if (meli.emit) {
						meli.emit('content:change');
					}
				} else {
					window.addEventListener('load', removeLoadingClass);
				}
			}

			function requestDescription(descContent) {
				var description = document.querySelector('.item-description');
				tiny.ajax('/noindex/description/MLB923783488?noIndex=true&forceHtml=true', {
					success: function(res) {
						removeLoadingClass();
						tiny.addClass(description, 'main-section--gradient');
						if (descContent) {
							descContent.innerHTML = res;
							if (meli.iframeModal) {
								meli.iframeModal.init();
							}
							meli.utils.lazy();
							meli.emit('content:change');
						}
					}
				});
			}

			var imageFailedLoading = false;
			exports.onImageError = function() {
				if (!imageFailedLoading) {
					var descContent = document.querySelector('.item-description__content');
					if (exports.tiny) {
						requestDescription(descContent);
					} else {
						window.addEventListener('load', requestDescription.bind(null, descContent));
					}
				}
				imageFailedLoading = true;
			};

			exports.onImageLoad = function(link) {
				var descImage = document.querySelector('.item-description__img');
				descImage.src = link.href;
				removeLoadingClass();

				var betterLink = doc.createElement('link');
				betterLink.href = link.href + '&quality=90';
				betterLink.rel = link.rel;
				betterLink.as = link.as;
				betterLink.onload = function() {
					descImage.src = this.href;
				};
				doc.head.appendChild(betterLink);
			};
		})(window, document);
	</script> 
