	window.vip = window.vip || {},
	window.vip.async = window.vip.async || {};

	function setStatus(status) {
		window.vip.async["recommendedItemsCarrousel"] = status;
	}

	function getStatus() {
		return window.vip.async["recommendedItemsCarrousel"];
	}

	(function loadAsyncCarousel(win, doc){
		if( getStatus() == undefined ) {
			setStatus('pending');
		}

		function loadCarousel() {
			if( getStatus() === 'loaded' ) {
				return;
			}

		
			var scrollTop = win.pageYOffset || doc.documentElement.scrollTop;

			if ( getStatus() === 'loading' || scrollTop < 500 ) {
				return;
			}
		

			var container = doc.querySelector('#recommendedItemsCarrousel');

			if( container ) {
				setStatus('loading');
				tiny.ajax('/noindex/moreitems/classifiedsSimilar?noIndex=true&itemId=MLB923783488&vertical=motors', {
					headers: { 'X-Requested-With': 'XMLHttpRequest' },
					success: function(data) {
						var content = data.trim();

						container.innerHTML = content;

					
						if( content ) {
							

							

							
								tiny.off(win, 'scroll', loadCarousel);
							

							var asyncCarousel = doc.querySelector('#recommendedItemsCarrousel .async-carousel'),
								carouselItems = new ch.Carousel(asyncCarousel, {
								
									
										pagination: false,
										autoHeight: false,
										autoMargin: false,
									
									limitPerPage: 5
								
								});
							setStatus('loaded');
						}
					
					},
					error: function(){
						setStatus('pending');
						throw new Error('Error loading carousel "#recommendedItemsCarrousel".')
					}
				});
			}
		}

	
		tiny.on(win, 'scroll', loadCarousel);
	

	})(window, document);
