         <script>
			(function(doc, win) {
				var html = doc.querySelector('.item-description__html-text');

				function calculateMaxWidth(elem) {
					var maxWidth = elem.offsetWidth;
					for (var i = 0, n = elem.children.length; i < n; i++) {
						maxWidth = Math.max(maxWidth, calculateMaxWidth(elem.children[i]));
					}
					return maxWidth;
				}

				function resizeHtmlDescription() {
					html.style.transform = '';
					html.style.height = '';

					// 1180 = legacy description max-width
					var maxWidth = Math.min(1180, calculateMaxWidth(html));
					var ratio = html.offsetWidth / maxWidth;

					html.style.transform = 'scale(' + ratio + ')';
					html.style.height = html.offsetHeight * ratio + 'px';
					if (win.meli && meli.emit) {
						meli.emit('content:change');
					}
				}

				window.addEventListener('resize', resizeHtmlDescription);
				var images = html.querySelectorAll('img');
				for (var i = 0, n = images.length; i < n; i++) {
					images[i].addEventListener('load', resizeHtmlDescription);
				}
				resizeHtmlDescription();
			})(document, window);
		</script> 
