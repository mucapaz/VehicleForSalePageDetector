	(function checkRedirectedHash(doc) {
		var insertMessage = function (message, link, linkText) {
			var container = doc.querySelector('.item-status-notification__title');
			var parent = doc.querySelector('.item-status--redirect');

			if (container) {
				container.innerHTML = message;
			}

			if (link) {
				var a = doc.createElement('a');
				a.href = link;
				a.className = 'u-link redirect';
				a.rel = 'nofollow';
				a.textContent = linkText;

				container.appendChild(a);
			}

			if (parent) {
				parent.style.display = 'block';
			}
		};

		if( "#redirectedFromParent" == window.location.hash ) {
		
		} else if( "#redirectedFromSimilar" == window.location.hash ) {
			insertMessage("VocÃª estÃ¡ vendo um novo anÃºncio, pois o produto que queria visitar finalizou.");
		} else {
			var similarWithId = window.location.hash.match(/^#redirectedFromSimilar=(.*)/);

			if( similarWithId ) {
				var itemUri = decodeURIComponent(similarWithId[1]) + '?redirectedFromSimilar=true';

				var lnk = doc.createElement('a');

				lnk.href = itemUri;

				if( lnk.hostname == doc.location.hostname ) {
					insertMessage("VocÃª estÃ¡ vendo um novo anÃºncio, pois o produto que queria visitar finalizou.", itemUri, "Ver produto finalizado");
				}
			}
		}
	})(window.document);
