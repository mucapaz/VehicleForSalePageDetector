        function getSubdomain() {
        var dom = "" + window.location.host;
        var pos = dom.indexOf("mercadoli");
        if (pos == -1)
          pos = dom.indexOf("deremate");
        if (pos == -1)
          pos = dom.indexOf("arremate");
        if (pos != -1)
          dom = dom.substring(pos);
        if (dom.indexOf(":") >= 0) {
          dom = dom.substring(0, dom.indexOf(":"));
        }

        return dom;
      }

      function setCookie(cookieName, cookieValue, nDays) {

        var domain = getSubdomain();
        var path = "/";

        if (nDays != null) {

          today = new Date();

          expire = new Date();
          if (nDays == null || nDays == 0)
            nDays = 1;
          expire.setTime(today.getTime() + 3600000 * 24 * nDays);

          document.cookie = cookieName + "=" + cookieValue + ";path=" + path + ";domain=." + domain + ";expires=" + expire.toGMTString();
        } else {
          document.cookie = cookieName + "=" + cookieValue + ";path=" + path + ";domain=." + domain;
        }
      }


      function performNaturalCheck(referrer, currentUrl, word) {
        if (!word) {
          return;
        }

        ind = getNaturalReferrerInd(referrer, currentUrl);

        if (ind == -1) {
          return;
        }
        naturalPmsSiteId = naturalPmsSiteIds[ind];

        new Image().src = "https://pmstrk." + _getUrlSubdomain(window.location.href) + '/jm/PmsTrk?tool=' + naturalPmsSiteId + '&word=' + word + '&go=%2Forg-img%2Fpixel.gif';
      }

      function _getUrlSubdomain(url) {
        var from = url.indexOf(".");
        var to = url.indexOf("/", from + 1);
        if (to == -1) {
          to = url.length;
        }
        return url.substring(from + 1, to);
      }

      function getCookieValueCall1(name) {
        var start = document.cookie.indexOf(name + "=");
        var len = start + name.length + 1;
        if (start == -1) return null;
        var end = document.cookie.indexOf(";", len);
        if (end == -1) end = document.cookie.length;
        return unescape(document.cookie.substring(len, end));
      }

      function getNaturalReferrerInd(referrer, currentUrl) {
        fromPms = getCookieValueCall1("pmsonline");
        //document.write("<BR>pmsonline:"+fromPms+"<BR>");
        if (fromPms != null) {
          if (fromPms == "YES") {
            setCookie("pmsonline", currentUrl, null);
            return -1;
          }
          if (fromPms.toLowerCase() == currentUrl.toLowerCase())
            return -1;
        }

        iQueryIndex = referrer.indexOf("?");
        if (iQueryIndex != -1)
          referrer = referrer.substring(0, iQueryIndex);
        referrer = referrer.toLowerCase();
        finded = -1;
        for (var i = naturalSites.length - 1; i > -1 && finded == -1; i--) {
          if (referrer.indexOf(naturalSites[i]) != -1)
            finded = i;
        }
        return finded;
      }

      function installListeners() {
        document.onclick = clickEvent;
        document.onkeypress = keyPressEvent;
      }

      function keyPressEvent(E) {
        performPopPms();
      }

      function clickEvent(E) {
        performPopPms();
      }

      function performPopPms() {
        var t_orgpms = getCookieValueCall1("t_orgpms");
        if (t_orgpms) {
          var t_cust_id = getCookieValueCall1("t_cust_id");
