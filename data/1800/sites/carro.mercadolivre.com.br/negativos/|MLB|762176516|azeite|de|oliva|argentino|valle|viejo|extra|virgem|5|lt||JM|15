       <section class="carousel-seller__section"> 
        <div id="recommendationsCarousel-1" class="carousel-common__section carousel-common--with-installments"></div> 
        <script type="text/javascript">
    window.addEventListener('load', function () {
        (function loadAsyncCarousel(win, doc) {

            function isChicoLoaded() { return typeof win.ch !== 'undefined'; }
            function initCarousel() {
                new ch.Carousel(document.querySelector('#recommendationsCarousel-1 .async-carousel'), {
                    
                    autoHeight: false,
                    autoMargin: false,
                    limitPerPage: 5,
                    
                    pagination: false
                });
            }

            function loadCarousel() {

                function trackMelidata(trackInfo) {
                    var recommendationsTrack = {
                        hidden_by_client: trackInfo.hiddenByClient,
                        client: trackInfo.client
                    };

                    if (!trackInfo.hiddenByClient) {

                        Object.assign(recommendationsTrack, {
                            has_errors: trackInfo.hasErrors
                        });

                        if (!trackInfo.hasErrors) {
                            Object.assign(recommendationsTrack, {
                                recommendation_id: trackInfo.recommendationId,
                                backend_id: trackInfo.backendId,
                                track_info: trackInfo.trackInfo
                            });

                            var experiment = {};

                            //TODO: deprecar experiment y dejar solo experiments cuando la migracion este completa
                            if (trackInfo.experiments) {
                                for (var i = 0, exp; i < trackInfo.experiments.length; i++) {
                                    exp = trackInfo.experiments[i]
                                    experiment[exp.name] = exp.variantId.toString();
                                }
                                melidata("add", "experiments", experiment);
                            } else if (trackInfo.experiment) {
                                experiment[trackInfo.experiment.name] = trackInfo.experiment.variantId.toString();
                                melidata("add", "experiments", experiment);
                            }
                        }
                    }


                    var trackData = {recommendations: recommendationsTrack};
                    melidata("cleanAndSend", "view", {
                        path: "/recommendations",
                        data: trackData
                    });
                }

                
                var url = "/noindex/moreitems/recommendationsSellerItems?noIndex=true&site_id=MLB&client=vip-seller_items-above&purchased_category=MLB1429&item_id=MLB762176516&user_id=&d2_id=e02221c8-30af-4d93-9e6f-97fb44241225&limit=20&ignore_min_reco_size=false&id=recommendationsCarousel-1";
                tiny.ajax(url, {
                    success: function (data) {
                        var content = data.html;
                        var trackInfo = data.trackInfo;
                        var items = trackInfo.items;
                        var itemWithInstallments = false;
                        if (items) {
                            [].forEach.call(items, function (item) {
                                if (item.site_id === "MLA") {
                                    if (item.installments && item.installments.no_interest) {
                                        itemWithInstallments = true;
                                        return;
                                    }
                                } else if (item.installments) {
                                        itemWithInstallments = true;
                                        return;
                                }
                            });
                            if (!itemWithInstallments) {
                                document.getElementById("recommendationsCarousel-1").classList.remove("carousel-common--with-installments")
                            }
                        }
                        if (trackInfo) {
                            trackMelidata(trackInfo);
                        }
                        if (content &&  content.trim()) {
                            content = content.trim();
                            var container = doc.getElementById("recommendationsCarousel-1");
                            container.innerHTML = content;
                            
                                if (isChicoLoaded()) {
                                    initCarousel();
                                } else {
                                    var timer = setInterval(function() {
                                        if (isChicoLoaded()) {
                                            initCarousel();
                                            clearInterval(timer);
                                        }
                                    }, 50);
                                }
                            
                            
                        }
                    },
                    error: function () {
                        throw new Error('Error loading carousel.')
                    }
                });

                

            }

            
            loadCarousel();
            
        })(window, document);
    });
</script> 
       </section> 
