                                        itemWithInstallments = true;
                                        return;
                                }
                            });
                            if (!itemWithInstallments) {
                                document.getElementById("recommendationsCarousel-3").classList.remove("carousel-common--with-installments")
                            }
                        }
                        if (trackInfo) {
                            trackMelidata(trackInfo);
                        }
                        if (content &&  content.trim()) {
                            content = content.trim();
                            var container = doc.getElementById("recommendationsCarousel-3");
                            container.innerHTML = content;
                            
                                if (isChicoLoaded()) {
                                    initCarousel();
                                } else {
                                    var timer = setInterval(function() {
                                        if (isChicoLoaded()) {
                                            initCarousel();
                                            clearInterval(timer);
                                        }
                                    }, 50);
                                }
                            
                            
                        }
                    },
                    error: function () {
                        throw new Error('Error loading carousel.')
                    }
                });

                
                tiny.off(win, 'scroll', loadCarousel);
                

            }

            
            tiny.on(win, 'scroll', loadCarousel);
            
        })(window, document);
    });
</script> 
