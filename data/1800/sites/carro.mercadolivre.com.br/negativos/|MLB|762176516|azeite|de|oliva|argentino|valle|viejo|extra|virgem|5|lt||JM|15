	(function (win, doc) {
		var ls,
			lsFallback = {},
			basePath = 'https://http2.mlstatic.com/ui/webfonts/v3.0.0/',
			FONTS_VERSION = 'v3.0.0',
			userAgent = navigator.userAgent,
			isOutdated = !win.addEventListener || userAgent.match(/(Android (2|3|4.0|4.1|4.2|4.3))|(Opera (Mini|Mobi))/) && !userAgent.match(/Chrome/),
			supportsWoff2 = (function () {
				if (!win.FontFace&& typeof win.FontFace !== 'function') {
					return false;
				}

				var font = new FontFace('test', 'url("data:application/font-woff2;base64,d09GMgABAAAAAADcAAoAAAAAAggAAACWAAEAAAAAAAAAAAAAAAAAAAAAAAAAAAAABk4ALAoUNAE2AiQDCAsGAAQgBSAHIBtvAcieB3aD8wURQ+TZazbRE9HvF5vde4KCYGhiCgq/NKPF0i6UIsZynbP+Xi9Ng+XLbNlmNz/xIBBqq61FIQRJhC/+QA/08PJQJ3sK5TZFMlWzC/iK5GUN40psgqvxwBjBOg6JUSJ7ewyKE2AAaXZrfUB4v+hze37ugJ9d+DeYqiDwVgCawviwVFGnuttkLqIMGivmDg") format("woff2")', {});
				font.load()['catch'](function () {});

				return /load(ed|ing)/.test(font.status);
			})();

		ls = getLocalStorage();

		/* Woff font loader without FOUT or FOIT and with localStorage caching */
		function loadFont(name, variants) {
			var fontId = name + '/' + variants,
				url;

			url = basePath + fontId;

			if (isOutdated) {
				var link = doc.createElement('link');
				link.setAttribute('rel', 'stylesheet');
				link.setAttribute('type', 'text/css');
				link.setAttribute('href', url + '.css');
			} else {
				var key = 'x-font-' + fontId,
					keyVersion = key + '-v',
					keyCss = key + '-css',
					currentVersion = ls[keyVersion],
					currentCss = ls[keyCss],
					style = doc.createElement('style');

				style.rel = 'stylesheet';
				doc.head.appendChild(style);

				if (!currentCss || currentVersion != FONTS_VERSION) {
					var loadFont = function() {
						var xhr = new XMLHttpRequest;

						xhr.onload = function () {
							if (xhr.status >= 200 && xhr.status < 400) {
								style.textContent = xhr.responseText;

								try {
									ls[keyVersion] = FONTS_VERSION;
									ls[keyCss] = xhr.responseText;
								} catch (e) {
									// 22: default DOMException.QUOTA_EXCEEDED_ERR
									// 1014: alternative FF NS_ERROR_DOM_QUOTA_REACHED
									if (e.code === 22 || e.code === 1014) {
										ls.clear();
										ls[keyVersion] = FONTS_VERSION;
										ls[keyCss] = xhr.responseText;
									}
								}
							}
						};

						xhr.open('GET', url + '.woff' + (supportsWoff2 ? '2' : '') + '.css');
						xhr.send();
					};

					if (doc.readyState === 'complete') {
						loadFont();
					} else {
						window.addEventListener('load', loadFont);
					}
				} else {
					style.textContent = currentCss;
				}
			}
		}

		/* Check for the localStorage support */
		function getLocalStorage() {
			var ls,
				lsCheck,
				lsCheckKey = '_t_';

			try {
				ls = win.localStorage;
				ls.setItem(lsCheckKey, 1);
				lsCheck = ls.getItem(lsCheckKey);
				ls.removeItem(lsCheckKey);
				if (!lsCheck) {
					ls = lsFallback;
				}
			} catch (e) {
				ls = lsFallback;
			}

			return ls;
		}

		
		loadFont('proxima-nova', '300-400-600-700');
		
	})(window, document);
