	(function vipMessages(window, document, meli) {
		'use strict';

		function Notice(m, c, t) {
			this.message = m;
			this.container = c;
			this.type = t;

			this.notice = function () {
				if (this.type == 'question') {
					meli_ga('send','event','ITEM','QUESTION','VIP');
					melidata('cleanAndSend', 'event', {
				   		path:'/questions/ask/post',
				   		data: {
							failed: 'false'
						}
					});
				}

				var p = document.createElement('p'),
						span = document.createElement('span');

				p.className = "ch-box-ok ch-box-icon";
				p.innerHTML = this.message;
				p.style = 'margin: 0; text-align: center; border-radius: 0; -webkit-border-radius: 0px; -moz-border-radius: 0px;';
				p.id = 'notice';

				span.style = 'font-weight: bold; position: absolute; top: 0px; right: 5px; font-size: 10px; cursor: pointer; padding: 5px;'
				span.innerHTML = 'X';

				p.appendChild(span);

				tiny.on(span, 'click', function() {
					p.style = 'display: none;';
				});

				if (this.container) {
					if (this.container.firstChild) {
						this.container.insertBefore(p, this.container.firstChild);
					} else {
						this.container.appendChild(p);
					}
				}
			};
		}

