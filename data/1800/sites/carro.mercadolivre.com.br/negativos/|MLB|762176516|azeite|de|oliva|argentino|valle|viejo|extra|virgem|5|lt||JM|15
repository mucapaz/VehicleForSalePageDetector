		notices['#REG_QUESTION_EMAIL=OK'] = new Notice('<strong>Já enviamos a sua pergunta!</strong> Avisaremos quando a responderem.', reference, 'question');

		if( hash != '' ) {
			if( notices[hash] != undefined ) {
				var notice = notices[hash];
				notice.notice();
			}
		}

		window.Notice = Notice;

