                            } else if (trackInfo.experiment) {
                                experiment[trackInfo.experiment.name] = trackInfo.experiment.variantId.toString();
                                melidata("add", "experiments", experiment);
                            }
                        }
                    }


                    var trackData = {recommendations: recommendationsTrack};
                    melidata("cleanAndSend", "view", {
                        path: "/recommendations",
                        data: trackData
                    });
                }

                
                var scrollTop = win.pageYOffset || doc.documentElement.scrollTop;

                if (scrollTop < 500) {
                    return;
                }
                
                var url = "/noindex/moreitems/recommendations?noIndex=true&site_id=MLB&client=vip&purchased_category=MLB1429&item_id=MLB762176516&user_id=&d2_id=e02221c8-30af-4d93-9e6f-97fb44241225&limit=20&id=recommendationsCarousel-3";
                tiny.ajax(url, {
                    success: function (data) {
                        var content = data.html;
                        var trackInfo = data.trackInfo;
                        var items = trackInfo.items;
                        var itemWithInstallments = false;
                        if (items) {
                            [].forEach.call(items, function (item) {
                                if (item.site_id === "MLA") {
                                    if (item.installments && item.installments.no_interest) {
                                        itemWithInstallments = true;
                                        return;
                                    }
