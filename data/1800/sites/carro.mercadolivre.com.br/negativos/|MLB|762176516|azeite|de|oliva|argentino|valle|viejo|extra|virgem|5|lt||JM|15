				notices = {},
				reference = document.getElementsByClassName('nav-main-content')[0];

		notices['#REG_FAVORITE=OK'] = new Notice('<strong>Salvamos o seu favorito!</strong> Você o encontrará na sua conta quando precisar.', reference, 'favorite');
