   <script>
(function(){
    var previewEl = document.querySelector('.heading__hero-preview');
    var headingEl = document.querySelector('.heading');
    var heroEl = document.createElement('div');
    var imgLarge = new Image();
    var largeSrc = previewEl.getAttribute('data-large');
    var transition = (function () {
        var s = document.createElement('div').style;
        var transEndEventNames = {
            WebkitTransition : 'webkitTransitionEnd',
            MozTransition    : 'transitionend',
            OTransition      : 'oTransitionEnd otransitionend',
            transition       : 'transitionend'
        };

        for (var name in transEndEventNames) {
            if (transEndEventNames.hasOwnProperty(name) && s[name] !== undefined) {
                return {
                    end: transEndEventNames[name]
                };
            }
        }

        return false;
    })();
    var checkCounter = 0;
    var checkLimit = 100;
    var checkHandler;

    function init() {
        imgLarge.src = largeSrc;

        if (imgLarge.complete) {
            heroEl.className += ' ready';
        } else {
            imgLarge.onload = function () {
                heroEl.className += ' loaded';
            };
        }

        heroEl.className += ' heading__hero';
        heroEl.style.backgroundImage = 'url(' + largeSrc + ')';

        headingEl.insertBefore(heroEl, previewEl.nextSibling);

        if (transition) {
            heroEl.addEventListener(transition.end, function(e) {
                previewEl.parentNode.removeChild(previewEl);
            });
        } else {
            previewEl.parentNode.removeChild(previewEl);
        }
    }

    checkHandler = setInterval(function() {
        checkCounter += 5;

        if (document.readyState === 'complete') {
            clearInterval(checkHandler);
            return init();
        }

        if (checkCounter > checkLimit) {
            clearInterval(checkHandler);
            previewEl.className += ' ready';
            if (window.addEventListener) {
                window.addEventListener('load', init, false);
            } else {
                window.attachEvent('onload', init);
            }
        }
    }, 5);
})();
</script> 
