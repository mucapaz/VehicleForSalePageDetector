  <script>
			(function checkRedirectedHash(doc) {
				var insertMessage = function (message, link, linkText) {
					var p = doc.createElement('p');

					p.textContent = p.innerText = message + " ";

					if( link ) {
						var a = doc.createElement('a');

						a.href = link;
						a.rel = 'nofollow';

						a.textContent = a.innerText = linkText;

						p.appendChild(a);
					}

					var container = document.getElementById('redirectedMsg');

					container.appendChild(p);

					container.className = container.className.replace('ch-hide', '');
				};

				if( "#redirectedFromParent" == window.location.hash ) {
			
				insertMessage("Você está vendo um novo anúncio do mesmo vendedor, pois o produto que queria visitar finalizou.", "/MLB-905289396-_JM?redirectedFromChild=MLB917647771", "Ver produto finalizado");
			
			} else if( "#redirectedFromSimilar" == window.location.hash ) {
				insertMessage("Você está vendo um novo anúncio, pois o produto que queria visitar finalizou.");
			} else {
				var similarWithId = window.location.hash.match(/^#redirectedFromSimilar=(.*)/);

				if( similarWithId ) {
					var itemUri = decodeURIComponent(similarWithId[1]) + '?redirectedFromSimilar=true';

					var lnk = doc.createElement('a');

					lnk.href = itemUri;

					if( lnk.hostname == document.location.hostname ) {
						insertMessage("Você está vendo um novo anúncio, pois o produto que queria visitar finalizou.", itemUri, "Ver produto finalizado");
					}
				}
			}
		})(window.document);
		</script>   
 </body>
</html>
