        options = {
            index: 0,
            history: false,
            focus: false,
            showAnimationDuration: 1000,
            hideAnimationDuration: 500,
            closeOnScroll: false,
            clickToCloseNonZoomable: false,
            preload: [1, 3],
            bgOpacity: 0.97
        };

    

    gallery = null;

    function openImagesVip(event) {
        var srcElement = event.target;

        if (!srcElement.hasAttribute('data-imgIndex')) {
            srcElement = srcElement.querySelector('[data-imgIndex]');
        }

        var picSelected = getSelectedImg(srcElement);

        // Update index only if was triggered by a thumb o main img
        if (picSelected != undefined) {
            options.index = parseInt(picSelected);
        }

        gallery = new PhotoSwipe(pswpElement, PhotoSwipeUI_Default, items, options);

        gallery.listen('beforeChange', function() {
            var idx = gallery.getCurrentIndex();
            if (hasVideo) {
                if (idx === videoSlidePosition) {
                    initYTPlayer();
                } else {
                    pauseYTPlayer(idx > videoSlidePosition + 1 || idx < videoSlidePosition - 1);
                }
            }
        });
        gallery.listen('close', function() {
            if (ytPlayer) {
                pauseYTPlayer(true);
            }
        });

        gallery.init();
    }

    function getSelectedImg(el) {
        return el.getAttribute('data-imgIndex');
    }

    for (var i = 0; i < classname.length; i++) {
        classname[i].addEventListener('click', openImagesVip, false);
    }

    function initYTPlayer() {
        if (!ytPlayer) {
            if (typeof YT === 'undefined' || !YT.Player) {
                window.onYouTubeIframeAPIReady = function() {
                    _initYTPlayer();
                }
            } else {
                _initYTPlayer();
            }
        }
    }

    function _initYTPlayer() {
        ytPlayer = new YT.Player('yt-player', {
            events: {
                'onReady': function() {
                    if (playedVideoTime) {
                        ytPlayer.seekTo(playedVideoTime);
                        ytPlayer.pauseVideo();
                    }
                },
                'onStateChange': function(e) {
                    var s = YT.PlayerState;

                    switch(e.data) {
                        case s.ENDED:
                        case s.UNSTARTED:
                            isVideoPlayed = false;
                            playedVideoTime = 0;
                            break;
                        case s.PAUSED:
                            isVideoPlayed = true;
                            playedVideoTime = ytPlayer.getCurrentTime();
                            break;
                        default:
                            isVideoPlayed = true;
                            break;
                    }
                }
            }
        });
    }

    function pauseYTPlayer(destroy) {
        if (ytPlayer) {
            if (isVideoPlayed) {
                ytPlayer.pauseVideo();
            }

            if (destroy) {
                ytPlayer.destroy();
                ytPlayer = null;
            }
        }
    }

    function galleryHandler(event) {
        var srcElement = event.target;

        if (srcElement.id != 'short-description-gallery') {
            return;
        }
        openImagesVip(event);
    }

    if (shortDescriptionGallery != null) {
        shortDescriptionGallery.addEventListener('click', galleryHandler, false);
    }

