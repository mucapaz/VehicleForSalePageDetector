		var rand = Math.random();
		$("#captchaNews").attr("src", "http://www.autoshoppingcuritiba.com.br//img/img.php?r=" + rand);
		$("#captchaNews2").attr("src", "http://www.autoshoppingcuritiba.com.br//img/img.php?r="+ rand);
        if($('#tmp_val_time').val() == '0'){
            $('#tmp_val_time').val(new Date().getTime());
