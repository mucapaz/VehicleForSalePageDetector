        googletag.defineSlot('/3981815/Sky_Esquerdo-Santos', [120, 600], 'div-gpt-ad-sky-left-Santos').addService(googletag.pubads());
        googletag.defineSlot('/3981815/dhtml-Santos', [300, 250], 'div-gpt-ad-dhtml-Santos').addService(googletag.pubads());

            var arrobaMapping = googletag.sizeMapping().
    addSize([980, 690], [300, 250]).
    addSize([768, 1024], [468, 60]).
    addSize([320, 480], [468, 60]).
    build();
    googletag.defineSlot('/3981815/arroba', [300, 250], 'div-gpt-ad-arroba').defineSizeMapping(arrobaMapping).addService(googletag.pubads());

    googletag.pubads().setTargeting("page", "motos");
    googletag.pubads().setTargeting("categories", "motos");


        googletag.pubads().enableSingleRequest();
        googletag.enableServices();
    });
</script> 
