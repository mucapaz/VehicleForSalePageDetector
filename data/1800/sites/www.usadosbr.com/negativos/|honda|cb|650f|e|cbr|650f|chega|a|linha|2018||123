                        //small image slide
                        jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-2 .td-button").each(function(i) {
                            jQuery(this).bind("click", function() {
                                jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-1").iosSlider("goToSlide", i+1);
                            });
                        });

                        //check the number of slides
                        if(td_uid_1_59e3ca502f68e_nr_of_slides > 7) {
                            jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-2").iosSlider({
                                desktopClickDrag: true,
                                snapToChildren: true,
                                snapSlideCenter: true,
                                infiniteSlider: true
                            });
                        } else {
                            jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-2").addClass("td_center_slide2");
                        }

                        function doubleSlider2Load_td_uid_1_59e3ca502f68e(args) {
                            //var currentSlide = args.currentSlideNumber;
                            jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-2").iosSlider("goToSlide", args.currentSlideNumber);


                            //put a transparent border around all small sliders
                            jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-2 .td-button .td-border").css("border", "3px solid #ffffff").css("opacity", "0.5");
                            jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-2 .td-button").css("border", "0");

                            //put a white border around the focused small slide
                            jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-2 .td-button:eq(" + (args.currentSlideNumber-1) + ") .td-border").css("border", "3px solid #ffffff").css("opacity", "1");
                            //jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-2 .td-button:eq(" + (args.currentSlideNumber-1) + ")").css("border", "3px solid #ffffff");

                            //write the current slide number
                            td_gallery_write_current_slide_td_uid_1_59e3ca502f68e(args.currentSlideNumber);
                        }

                        //writes the current slider beside to prev and next buttons
                        function td_gallery_write_current_slide_td_uid_1_59e3ca502f68e(slide_nr) {
                            jQuery("#td_uid_1_59e3ca502f68e .td-gallery-slide-item-focus").html(slide_nr);
                        }


                        /*
                        * Resize the iosSlider when the page is resided (fixes bug on Android devices)
                        */
                        function td_gallery_resize_update_vars_td_uid_1_59e3ca502f68e(args) {
                            if(tdDetect.isAndroid) {
                                setTimeout(function(){
                                    jQuery("#td_uid_1_59e3ca502f68e .td-doubleSlider-1").iosSlider("update");
                                }, 1500);
                            }
                        }
                    });

                    //total number of slides
                    var td_uid_41_59e3ca5086f55_nr_of_slides = 6;

                    jQuery(document).ready(function() {
                        //magnific popup
                        jQuery("#td_uid_41_59e3ca5086f55 .td-slide-popup-gallery").magnificPopup({
                            delegate: "a",
                            type: "image",
                            tLoading: "Loading image #%curr%...",
                            mainClass: "mfp-img-mobile",
                            gallery: {
                                enabled: true,
                                navigateByImgClick: true,
                                preload: [0,1],
                                tCounter: '%curr% of %total%'
                            },
                            image: {
                                tError: "<a href='%url%'>The image #%curr%</a> could not be loaded.",
                                    titleSrc: function(item) {//console.log(item.el);
                                    //alert(jQuery(item.el).data("caption"));
                                    return item.el.attr("data-caption") + "<div>" + item.el.attr("data-description") + "<div>";
                                }
                            },
                            zoom: {
                                    enabled: true,
                                    duration: 300,
                                    opener: function(element) {
                                        return element.find("img");
                                    }
                            },

                            callbacks: {
                                change: function() {
                                    // Will fire when popup is closed
                                    jQuery("#td_uid_41_59e3ca5086f55 .td-doubleSlider-1").iosSlider("goToSlide", this.currItem.index + 1 );
                                }
                            }

                        });

                        jQuery("#td_uid_41_59e3ca5086f55 .td-doubleSlider-1").iosSlider({
                            scrollbar: true,
                            snapToChildren: true,
                            desktopClickDrag: true,
                            infiniteSlider: true,
                            responsiveSlides: true,
                            navPrevSelector: jQuery("#td_uid_41_59e3ca5086f55 .doubleSliderPrevButton"),
                            navNextSelector: jQuery("#td_uid_41_59e3ca5086f55 .doubleSliderNextButton"),
                            scrollbarHeight: "2",
                            scrollbarBorderRadius: "0",
                            scrollbarOpacity: "0.5",
                            onSliderResize: td_gallery_resize_update_vars_td_uid_41_59e3ca5086f55,
                            onSliderLoaded: doubleSlider2Load_td_uid_41_59e3ca5086f55,
                            onSlideChange: doubleSlider2Load_td_uid_41_59e3ca5086f55,
                            keyboardControls:true
                        });

                        //small image slide
                        jQuery("#td_uid_41_59e3ca5086f55 .td-doubleSlider-2 .td-button").each(function(i) {
                            jQuery(this).bind("click", function() {
                                jQuery("#td_uid_41_59e3ca5086f55 .td-doubleSlider-1").iosSlider("goToSlide", i+1);
                            });
                        });

                        //check the number of slides
                        if(td_uid_41_59e3ca5086f55_nr_of_slides > 7) {
                            jQuery("#td_uid_41_59e3ca5086f55 .td-doubleSlider-2").iosSlider({
                                desktopClickDrag: true,
                                snapToChildren: true,
                                snapSlideCenter: true,
                                infiniteSlider: true
                            });
                        } else {
                            jQuery("#td_uid_41_59e3ca5086f55 .td-doubleSlider-2").addClass("td_center_slide2");
                        }

