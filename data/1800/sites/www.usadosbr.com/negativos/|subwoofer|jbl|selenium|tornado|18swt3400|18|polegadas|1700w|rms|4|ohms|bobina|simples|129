         <script>

                        jQuery(document).ready(function (){
                            jQuery(window).scroll(function (event) {

                                var page = jQuery(window);
                                var pageScroll = page.scrollTop()+jQuery('#barratopofix').height();
                                var pageTotalScroll = page.scrollTop() + page.height();

                                var firstScroll = jQuery('.divnomeproduto').offset().top;

                                var box = jQuery('.productBuyBoxFixed');

                                if (pageScroll > firstScroll){
                                    box.addClass('productBuyBoxFixedFixed');
                                    jQuery('.productBuyInfoBox .divproductname').css('display', 'block');
                                } else {
                                    box.removeClass('productBuyBoxFixedFixed');
                                    jQuery('.productBuyInfoBox .divproductname').css('display', 'none');
                                }

                                if(pageTotalScroll > jQuery('.stopFixing').offset().top){
                                    box.removeClass('productBuyBoxFixedFixed');
                                    box.addClass('productBuyBoxFixedBottom');
                                    jQuery('.productBuyInfoBox .divproductname').css('display', 'block');
                                } else if(pageScroll > firstScroll) {
                                    box.removeClass('productBuyBoxFixedBottom');
                                    box.addClass('productBuyBoxFixedFixed');
                                    jQuery('.productBuyInfoBox .divproductname').css('display', 'block');
                                } else {
                                    box.removeClass('productBuyBoxFixedBottom');
                                    jQuery('.productBuyInfoBox .divproductname').css('display', 'none');
                                }
                            });
                        });

                        </script> 
