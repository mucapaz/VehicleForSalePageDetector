                                    jQuery(document).ready(function (){

                                      jQuery("#letMeKnow img").remove();

                                        jQuery("#info .mais_formas_pagamentos").click(function (){

                                            if(jQuery(this).hasClass('animated')){
                                                jQuery(this).removeClass('animated');
                                                jQuery(this).animate({deg: 0}, {step: function(now,fx) {
                                                      jQuery(this).css('-webkit-transform','rotate('+now+'deg)');
                                                      jQuery(this).css('transform', 'rotate('+now+'deg)');
                                                    },
                                                    duration:500}, 'linear');
                                            } else {
                                                jQuery(this).addClass('animated');
                                                jQuery(this).animate({deg: 180}, {step: function(now,fx) {
                                                      jQuery(this).css('-webkit-transform','rotate('+now+'deg)');
                                                      jQuery(this).css('transform', 'rotate('+now+'deg)');
                                                    },
                                                    duration:500}, 'linear');
                                            }

                                            jQuery("#infoimg").slideToggle('fast');
                                        });
                                    });
                                </script> 
