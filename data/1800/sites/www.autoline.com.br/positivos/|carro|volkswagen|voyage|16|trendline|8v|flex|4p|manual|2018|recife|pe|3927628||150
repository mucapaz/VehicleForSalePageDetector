        function getCookie(chave) {
            var ChaveValor = document.cookie.match('(^|;) ?' + chave + '=([^;]*)(;|$)');
            return ChaveValor ? ChaveValor[2] : null;
        }

        function setCookie(name, value, exdays) {
            var expires;
            var date;

            date = new Date(); //  criando o COOKIE com a data atual
            date.setTime(date.getTime() + (exdays * 24 * 60 * 60 * 1000));
            expires = date.toUTCString();
            document.cookie = name + "=" + value + "; expires=" + expires + ";domain=.autoline.com.br; path=/";
        }

        var LIP_LowPrecision = false; //false = pede permissao pelo navegador, Maior Precisao | true = nao pede permissao, Menor Precisao
        function LocalizaIP_done(ip_data) {
            if (!ip_data['error']) //esta linha eh um exemplo, deve trocá-la pelo programa que irá manipular os dados de Geolocalizacao
            {
                if (getCookie('GeoUF') == null) {
                    setCookie('GeoUF', ip_data['state'], 87600000);
                    setCookie('GeoCity', ip_data['city'], 87600000);
                    setCookie('FonteGeoLocation', "ip", 87600000);
                }
            }
        }
