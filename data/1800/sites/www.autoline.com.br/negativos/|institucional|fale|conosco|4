   <script>
    function getCookie(chave) {
        var ChaveValor = document.cookie.match('(^|;) ?' + chave + '=([^;]*)(;|$)');
        return ChaveValor ? ChaveValor[2] : null;
    }
    var userId = $("#UserIdCh").val();
    if (userId !== "0" && userId !== "") {
        var userName = $("#UserNameCh").val();
        var userEmail = $("#UserEmailCh").val();
        var userAllowMail = $("#UserAllowMailCh").val().toLowerCase() === 'true';
        var userState = $("#UserStateCh").val();
        var userCity = $("#UserCityCh").val();

        if (userState == '') {
            userState = getCookie('GeoUF');
            userCity = getCookie('GeoCity');
        }

        window.chaordic_meta = {
            "page": "other",
            "tags": [userCity, userState],
            "user": {
                "id": userId,
                "name": userName,
                "email": userEmail,
                "allow_mail_marketing": userAllowMail
            }
        }
    } else {
        var ufch;
        var citych;

        if (getCookie('GeoUF') != null) {
            ufch = getCookie('GeoUF');
            citych = getCookie('GeoCity');
        }

        window.chaordic_meta = {
            "page": "other",
            "tags": [citych, ufch]
        }


    }

</script> 
