    $(this.document).ready(function () {
        $(".ordenarVeiculos li a").click(function () {
            $("#results-holder").load("~/Home/OrdenarVeiculos/" + $(this).data("id"), function () {
                $.unblockUI();
            });
        });

        loadSelect("/Home/ObterPrecosDe/1", null, "#precoDe", (0 == null ? "0" : 0), function () { });
        loadSelect("/Home/ObterPrecosAte/1", null, "#precoAte", (0 == null ? "0" : 0), function () {  });
        loadSelect("/Home/ObterAnosDe/1", null, "#anoDe", (0 == null ? "0" : 0), function () {  });
        loadSelect("/Home/ObterAnosAte/1", null, "#anoAte", (0 == null ? "0" : 0), function () {  });
        loadSelect("/Home/ObterLojas/1", null, "#loja", (0 == null ? "0" : 0), function () {  });

        loadSelect("/Home/ObterMarcasPorTipo/1", null, "#Marca", (0 == null ? "0" : 0), function () {  });

        $("#Marca").change(function () {
            loadSelect("/Home/ObterModelosPorMarca/" + $(this).val(), null, "#Modelo", (0 == null ? "0" : 0), function () {  });
        });
    });
