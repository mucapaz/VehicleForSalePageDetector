	function checaform1()
	{
		var erro="";
		if (window.document.form1.palavrachave.value == "")
		{
			erro += "Favor digitar um modelo";
		}
		if (erro == "")
		{
			return true
		}
		else
		{
			alert (erro);
			window.document.form1.palavrachave.focus();
			return false;
		}
	}
	
