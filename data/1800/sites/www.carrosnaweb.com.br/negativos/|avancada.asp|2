  <script src="scripts/ajaxfabricanteschangecatalogo.js"></script> 
  <script>
function OpenSel(op) 
{
	if (op == 1)
	{
		document.getElementById('mecanica').style.display="none";
		document.getElementById('mostrarmecanica').style.display="block";
	}
	if (op == 2) 
	{
		document.getElementById('mecanica').style.display="block";
		document.getElementById('mostrarmecanica').style.display="none";
	}
	if (op == 3) 
	{
		document.getElementById('seguranca').style.display="none";
		document.getElementById('mostrarseguranca').style.display="block";
	}
	if (op == 4) 
	{
		document.getElementById('seguranca').style.display="block";
		document.getElementById('mostrarseguranca').style.display="none";
	}
	if (op == 5)
	{
		document.getElementById('conforto').style.display="none";
		document.getElementById('mostrarconforto').style.display="block";
	}
	if (op == 6)
	{
		document.getElementById('conforto').style.display="block";
		document.getElementById('mostrarconforto').style.display="none";
	}
}
</script>
