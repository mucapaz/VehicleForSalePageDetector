  <script language="JavaScript">
	function MM_openBrWindow(theURL,winName,features) 
	{ 
		window.open(theURL,winName,features);
	}
	
function checa()
{
	var error_string = "";
	var PrimeiroErro = "";
	if (error_string == "")
	{
		return true;
	}
	else
	{
		alert(error_string);
		RecebeErro = 'document.form1.'+PrimeiroErro+'.focus();';
		eval(RecebeErro);
		return false;
	}	
}
</script> 
