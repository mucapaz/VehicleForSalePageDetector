  <script language="JavaScript">
	    function MM_openBrWindow(theURL,winName,features) 
	    { 
		    window.open(theURL,winName,features);
	    }
	    function Aspas(Tecla)
	    {
		    if (Tecla==39)
		    {
			    return false			
		    }
		    else
		    {
			    return true
		    }
	    }	
	    function checaform2()
	    {
		    var erro="";
		    if (window.document.form2.email.value == "")
		    {
			    erro += "Favor digitar o seu e-mail.";
		    }
		    else
		    {
			    var AtSym  = window.document.form2.email.value.indexOf('@')
			    var Period = window.document.form2.email.value.lastIndexOf('.')
			    var Space  = window.document.form2.email.value.indexOf(' ')
			    var Length = window.document.form2.email.value.length - 1
			    if ( (AtSym < 1) ||       // '@' cannot be in first position
			       (Period <= AtSym+1) || // Must be atleast one valid char btwn '@' and '.'
			       (Period == Length ) || // Must be atleast one valid char after '.'
		           (Space  != -1) )       // No empty spaces permitted
			    {  
				    erro += "Formato de e-mail incorreto.";
			    }
		    }		
		    if (erro == "")
		    {
			    return true
		    }
		    else
		    {
			    alert (erro);
			    window.document.form2.email.focus();
			    return false;
		    }
	    }
	
    </script> 
