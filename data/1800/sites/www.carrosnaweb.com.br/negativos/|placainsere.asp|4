function checa()
{
	var error_string = "";
	var PrimeiroErro = "";
	if (window.document.form2.valor.value =="")
	{
		error_string += "Favor preencher o valor.\n";
		PrimeiroErro = "valor";
	}
	if (window.document.form2.anomod.value =="-1")
	{
		error_string += "Favor selecionar o ano modelo.\n";
		PrimeiroErro = "anomod";
	}
	if (window.document.form2.anofab.value =="-1")
	{
		error_string += "Favor selecionar o ano fabrica��o.\n";
		PrimeiroErro = "anofab";
	}
	if (window.document.form2.combustivel.value =="-1")
