		error_string += "Favor preencher o seu nome.\n";
		PrimeiroErro = "nome";
	}
	if (error_string == "")
	{
		return true;
	}
	else
	{
		alert(error_string);
		RecebeErro = 'document.form2.'+PrimeiroErro+'.focus();';
		eval(RecebeErro);
		return false;
	}	
}
