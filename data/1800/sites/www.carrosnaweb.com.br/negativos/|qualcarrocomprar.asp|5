	function MM_openBrWindow(theURL,winName,features) 
	{ 
		window.open(theURL,winName,features);
	}
	
	function checaform1()
	{
		var erro="";
		if(document.form1.anofim.options[document.form1.anofim.selectedIndex].text<document.form1.anoini.options[document.form1.anoini.selectedIndex].text)
		{
			erro = "O ano final deve ser superior ao ano inicial.\n";
			PrimeiroErro = "anofim";
		}
		if (erro == "")
		{
			return true
		}
		else
		{
			alert (erro);
			window.document.form1.anofim.focus();
			return false;
		}
	}
	
