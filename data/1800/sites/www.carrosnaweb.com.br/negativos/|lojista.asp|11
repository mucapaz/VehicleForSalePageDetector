	{
		if (document.form1.k40fjhw32y.value.length<4)
		{
			error_string += "Senha deve ter mais de 3 caracteres.\n";
			PrimeiroErro = "k40fjhw32y"
		}
	}
	if (document.form1.a5f198sd4z.value =="")
	{
		error_string += "Favor preencher seu e-mail.\n";
		PrimeiroErro = "a5f198sd4z"
	}
	else
	{
		var AtSym  = document.form1.a5f198sd4z.value.indexOf('@')
		var Period = document.form1.a5f198sd4z.value.lastIndexOf('.')
		var Space  = document.form1.a5f198sd4z.value.indexOf(' ')
		var Length = document.form1.a5f198sd4z.value.length - 1
		if ( (AtSym < 1) ||       // '@' cannot be in first position
		   (Period <= AtSym+1) || // Must be atleast one valid char btwn '@' and '.'
		   (Period == Length ) || // Must be atleast one valid char after '.'
		   (Space  != -1) )       // No empty spaces permitted
		{  
			error_string += "Formato de e-mail incorreto.\n";
			if (PrimeiroErro == "")
			{
				PrimeiroErro = "a5f198sd4z"
			}
		}
