	function checaform1()
	{
		var erro="";
		if (window.document.form1.nome.value == "")
		{
			erro += "Favor digitar um modelo ou um fabricante.";
		}
		if (erro == "")
		{
			return true
		}
		else
		{
			alert (erro);
			window.document.form1.nome.focus();
			return false;
		}
	}
	
	function MM_openBrWindow(theURL,winName,features) 
	{ 
		window.open(theURL,winName,features);
	}
	
	function Aspas(Tecla)
	{
		if (Tecla==39)
		{
			return false			
		}
		else
		{
			return true
		}
	}

function checa(){
	var error_string = "";
	var PrimeiroErro = "";
	if (window.document.Carro.versao2.value == "-1")
	{
		error_string += "Favor selecionar a versão.\n";
		PrimeiroErro = ("versao2")
	}	
	if (window.document.Carro.ano2.value == "-1")
	{
		error_string += "Favor selecionar o ano.\n";
		PrimeiroErro = ("ano2")
	}	
	if (window.document.Carro.modelo2.value == "-1")
	{
		error_string += "Favor selecionar o modelo.\n";
		PrimeiroErro = ("modelo2")
	}	
	if (window.document.Carro.fabricante2.value == "-1")
	{
		error_string += "Favor selecionar o fabricante.\n";
		PrimeiroErro = ("fabricante2")
	}	
	if (window.document.Carro.versao1.value == "-1")
	{
	error_string += ("Favor selecionar a versão.\n");
	PrimeiroErro = "versao1";
	}
	if (window.document.Carro.ano1.value == "-1")
	{
	error_string += ("Favor selecionar o ano.\n");
	PrimeiroErro = "ano1";
	}
	if (window.document.Carro.modelo1.value == "-1")
	{
	error_string += ("Favor selecionar o modelo.\n");
	PrimeiroErro = "modelo1";
	}
	if (window.document.Carro.fabricante1.value == "-1")
	{
		error_string += "Favor selecionar o fabricante.\n";
		PrimeiroErro = ("fabricante1")
	}	
	if (window.document.Carro.versao1.value == window.document.Carro.versao2.value)
	{
		error_string += "Favor selecionar carros diferentes para realizar a comparação.\n";
		PrimeiroErro = ("fabricante1")
	}	
	if (error_string == "")
	{
	if (((Carro.versao3.value == "-1")||(Carro.versao3.value == ""))&&((Carro.versao4.value == "-1")||(Carro.versao4.value == "")))
		{
			Carro.action = 'resultcompara.asp?modelos='+Carro.versao1.value+'-'+Carro.versao2.value;
			return true;
		}
		else
		{
			if ((Carro.versao4.value == "-1")||(Carro.versao4.value == ""))
			{
				Carro.action = 'resultcompara.asp?modelos='+Carro.versao1.value+'-'+Carro.versao2.value+'-'+Carro.versao3.value;
				return true;
			}
			else
			{
				Carro.action = 'resultcompara.asp?modelos='+Carro.versao1.value+'-'+Carro.versao2.value+'-'+Carro.versao3.value+'-'+Carro.versao4.value;
				return true;
			}
		}
	}
	else
	{
		alert(error_string);
		RecebeErro = ('window.document.Carro.'+PrimeiroErro+'.focus();');
		eval(RecebeErro);
		return false;
	}	
}	
//-->

</script></font></font>
 </body>
</html>
