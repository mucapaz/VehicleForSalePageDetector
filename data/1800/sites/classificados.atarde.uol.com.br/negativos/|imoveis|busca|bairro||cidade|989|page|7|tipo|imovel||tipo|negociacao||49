    function habilitar_bairros() {
        if ($("#cidade").val() == '989') {   //SALVADOR
            $("#div_bairro").attr('style', 'display: block');
            $('#bairro').attr('disabled', false);
        } else {
            $("#div_bairro").attr('style', 'display: none');
            $('#bairro').attr('disabled', true);

