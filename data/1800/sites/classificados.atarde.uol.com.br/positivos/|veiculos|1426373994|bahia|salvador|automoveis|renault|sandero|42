      <script type="text/javascript">
        $(function () {
            $('#enviar_msg_anunciante').on('click', function () {
                if (!isEmpty(new Array('#nome_msg', '#telefone_msg', '#email_msg', '#mensagem_msg')) && isValidEmail('#email_msg')) {

                    $.ajax({
                        url: '/veiculos/mensagem_anunciante',
                        headers: {
                            Accept: "text/javascript; charset=utf-8",
                            "Content-Type": 'application/x-www-form-urlencoded; charset=UTF-8'
                        },
                        type: 'POST',
                        data: {
                            anunciante_email: $('#anunciante_email').val(),
                            anunciante_nome: $('#anunciante_nome').val(),
                            titulo_anuncio: $('#titulo_anuncio').val(),
                            anuncio_id: $('#anuncio_id').val(),
                            nome_msg: $('#nome_msg').val(),
                            email_msg: $('#email_msg').val(),
                            telefone_msg: $('#telefone_msg').val(),
                            horario_msg: $('#horario_msg:checked').map(function () {
                                return this.value;
                            }).get().join(","),
                            mensagem_msg: $('#mensagem_msg').val(),
                            copia_email_msg: $('#copia_email_msg:checked').val(),

                            authenticity_token: $(this).siblings('#authenticity_token').val()
                        }
                    });


                }
                return false;


            });
            $('#telefone_msg').mask('(99) 9999-9999');
        });

    </script> 
