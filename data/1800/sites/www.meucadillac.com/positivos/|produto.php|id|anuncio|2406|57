  <script>
		function updateTextArea() {     
		   var allVals = [];
		   $('.taglist :checked').each(function(i) {
				  
			   allVals.push((i!=0?"\r\n":"")+ $(this).val());
		   });
		   $('#mensagem').val(allVals).attr('rows',allVals.length) ;
			
		   }
		   $(function() {
			  $('.taglist input').click(updateTextArea);
			  updateTextArea();
		});
	</script> 
  <script>
		function alert()
		{
			if(document.getElementById('sou').checked == true){  
			
				document.getElementById("alert").disabled = true; //Desabilitando 
				document.getElementById('alert').style.display="none";
				
		    } else {    
				document.getElementById("alert").disabled = false; //Habilitando
				document.getElementById('alert').style.display="block";
		    }			
		}
	</script> 
