   <script type="text/x-tmpl" id="tmpl_plan">
	{% for (var plano = 0; plano < o.length; plano++) { %}
		<div class="app-plan-{%=o.colors[plano]%} box-flex">
			<div class="thumbnail">
				<div class="caption">
					<h3 class="plan-name">{%=o[plano].nome%}</h3>

					{% if (o.payment_method == 0) { %}
						<h2 class="price">{%=o[plano].preco_mensal%}</h2>
					{% } %}

					{% if (o.payment_method == 1) { %}
						<h2 class="price">{%=o[plano].preco_semestral%}</h2>
					{% } %}

					{% if (o.payment_method == 2) { %}
						<h2 class="price">{%=o[plano].preco_anual%}</h2>
					{% } %}

					{% if (o[plano].qtd_anuncios > 1000) { %}
						<p class="text-center">An&uacute;ncios ilimitados</p>
					{% } else { %}
						<p class="text-center">{%=o[plano].qtd_anuncios%} an&uacute;ncios (via cr&eacute;ditos) / m&ecirc;s</p>
					{% } %}

					<p class="text-center">An&uacute;ncios publicados no site at&eacute; ser exclu&iacute;do</p>
					<p class="text-center">Selo de Cliente Premium nos seus an&uacute;ncios e na sua loja</p>
					<!-- <p class="text-center">Importa&ccedil;&atilde;o de an&uacute;ncios (via integradores)</p>-->
					<p class="text-center">{%=o[plano].qtd_anuncios_destaques%} an&uacute;ncio "destaque" incluso</p>
					<p class="text-center">Pre&ccedil;o unit&aacute;rio por an&uacute;ncio "destaque": R$ {%=o[plano].preco_destaque%}</p>
					{% if (o[plano].tem_custom_url) { %}
						<p class="text-center">URL personalizada para a sua loja</p>
					{% } %}
					<p class="text-center"><a href="javascript:enviar({%=o[plano].id%})" class="btn" enviar role="button">EU QUERO!</a></p>
				</div>
			</div>
		</div>
	{% } %}
</script> 
   <script type="text/javascript">
		function loadPlans(categoria){
			jQuery.ajax({
				url: '/app/get_planos_pj',
				type: 'GET',
				dataType: 'json',
				data: {categoria: categoria},
				success: function(data, textStatus, xhr) {
					if (xhr.status == 204){
						utils.info('Plano não cadastrado', 3000, 'erro');
						jQuery('#box_plan').html('');
						return;
					}
					data.data.colors = ['orange', 'purple', 'green'];
					var idx_bloquear = -1;
					
					data.data.payment_method = jQuery('#payment_method').val();
					jQuery('#box_plan').html(tmpl("tmpl_plan", data.data));
					if (idx_bloquear > -1){
						$('.box-flex').find('a').eq(idx_bloquear).text('PLANO ATUAL');
						$('.box-flex').find('a').eq(idx_bloquear).attr('href', '#');
					}
				},
				error: function(xhr, textStatus, errorThrown) {
					utils.info(xhr.responseJSON.msg, 3000, 'error');
				}
			});
		}

		function enviar(id_plano){
			var renovacao = false;

			if(!renovacao){
				jQuery.ajax({
					url: '/planos_proc',
					type: 'GET',
					dataType: 'json',
					data: {
						metodo_pagamento: jQuery('#payment_method').val(),
						plano: id_plano,
						categoria: jQuery('.js_plan.active').attr('data-id')
					},
					success: function(data, textStatus, xhr) {
						if (data.logar){
							jQuery('#login-now').modal();
							jQuery('.js-login').attr('action', jQuery('.js-login').attr('action') + data.url);
						}else{
							if (data.cadastro){
								location.href = data.url;
							}else{
								location.href = data.url;
							}
						}
					},
					error: function(xhr, textStatus, errorThrown) {
						utils.info(xhr.responseJSON.msg, 3000, 'error');
					}
				});
			}else{
				utils.info('O plano escolhido é o mesmo que você já possui.', 3000, 'warning');
			}
		}

		jQuery('.js_plan').on('click', function(){
			jQuery('.js_plan').removeClass('active');
			jQuery(this).addClass('active');
			var id_categoria = jQuery(this).attr('data-id');
			loadPlans(id_categoria);
		});

		jQuery('#payment_method').on('change', function(){
			var id_categoria = jQuery('.js_plan.active').attr('data-id');
			loadPlans(id_categoria);
		});

		
			jQuery(function() {
				$('.js-login').find('input[type="submit"]').after('<a href="/anunciar/cadastro?r=/destaques" class="lose-password" tabindex="6">Não tem cadastro? Clique aqui</a>');
				jQuery('.js_plan:first').click();
			});
		
		$(document).ready(function () {
			
		});
	</script> 
