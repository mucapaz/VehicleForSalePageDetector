     <script type="text/x-tmpl" id="tmpl-widget-link">
	<div class="active">
		<label for="">{%=o.titulo%}</label>
		<select data-placeholder="{%=o.titulo%}" data-attribute="{%=o.nome%}" id="atributte-{%=o.nome%}" class="chosen-select js-link-filter"
			{% if(o.hasOwnProperty('disabled')) { %}disabled{% } %}
			{% if(o.nome in {'cidade': 1, 'situacao_veiculo': 1, 'caracteristicas': 1, 'fabricante': 1, 'modelo_base': 1}) { %}
				multiple
			{% } %} tabindex="4"
		>

		<option value="">Selecione</option>

		{% if(o.hasOwnProperty('pivot')) { %}
			{% for (var prop in o.valores) { %}
				<optgroup label="{%=o.valores[prop].name%}">
					{% for (var prop_filho in o.valores[prop].valor) { %}
						<option value="{%=o.valores[prop].valor[prop_filho].valor%}">{%=o.valores[prop].valor[prop_filho].name%}</option>
					{% } %}
				</optgroup>
			{% } %}
		{% } else { %}
			{% for (var prop in o.valores) { %}
