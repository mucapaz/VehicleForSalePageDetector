	function formata_descricao(descricao) {
		if (descricao.length > 150) {
			return descricao.slice(0, 150) + " ...";
		} else {
			return descricao
		}
	}

	var list;
	var current;
	var dimension2Value = '';

	// Verifying buttons status
	function verifyButtons(e) {
		if (current <= 0) {
			e.parent().find('.prev-img').attr("disabled", "disabled");
			e.parent().find('.next-img').attr("disabled", false);
			e.parent().addClass('first');
		}else if((current + 1) >= list.length){
			e.parent().find('.prev-img').attr("disabled", false);
			e.parent().find('.next-img').attr("disabled", "disabled");
			e.parent().addClass('last');
		}else{
			e.parent().find('.prev-img').attr("disabled", false);
			e.parent().find('.next-img').attr("disabled", false);
			e.parent().removeClass('first');
			e.parent().removeClass('last');
		}
	}

	// Changing image
	function changeImage(e) {
		current = e.parent().data('current');
		if (e.hasClass('next-img')) {
			current++;
			e.parent().data('current', current);
		} else {
			current--;
			e.parent().data('current', current);
		}
		list = e.parent().siblings('.image__list').data('images');
		if (typeof(list[current]) !== 'undefined') {
			e.parent().css("background-image", "url("+list[current]+")");
			e.parent().fadeIn(300);
		}
	}
</script> 
