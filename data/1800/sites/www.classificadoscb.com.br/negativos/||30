		data-atributo-lojalink="{%=o.link_loja%}"
	>
		<div class="image__list"
		data-images='[
			{% for (var i=0; i<o.fotos_formatadas.length; i++) { %}
				"{%=o.fotos_formatadas[i]%}"
				{% if (i < o.fotos_formatadas.length-1) { %},{% } %}
			{% } %}]'></div>

			{% if (o.preco_fmt) { %}
				<div class="classified__images first price" data-current="0">
				<a href="{%=o.link_detalhe%}" class="classified__images--price">
					{%=o.preco_fmt%}
				</a>
			{% } else { %}
				<div class="classified__images first" data-current="0">
			{% } %}
			{% if (o.fotos_formatadas.length > 1)  { %}
				<button class="prev-img prev" disabled="disabled"><i class="icons-arrow_back"></i></button>
				<button class="next-img next"><i class="icons-arrow"></i></button>
			{% } %}
		</div>
		{% if (o.publicar_contato == '1') { %}
			{% if (o.dono_tipo == 'Revenda') { %}
				<a href="{%=o.link_loja%}" class="classified__content--owner">
					<div class="owner__shop">
						{% if (o.dono_image != '') { %}
							<img src="{%=o.dono_image %}">
						{% } else { %}
							<div class="owner__name">{%=o.dono_nome%}</div>
						{% } %}
					</div>
				</a>
			{% } else { %}
				<a href="{%=o.link_loja%}" alt="" title="" class="classified__content--owner">
					{% if (o.dono_image) { %}
						<div class="owner__photo" style="background-image: url('{%=o.dono_image %}');"></div>
					{% } %}
					<div class="owner__name">{%=o.dono_nome%}</div>
				</a>
			{% } %}
		{% } %}
		<a href="{%=o.link_detalhe%}">
			<div class="classified__content">
				<span class="classified__content--title">{%=o.titulo%}</span>
				{% if (o.categoria_slug == 'imoveis') { %}
					<span class="classified__content--location">
						<i class="icons-icon-info-small-grey"></i>
						{% if (o.secao == 'Compra e Venda') { %}
							Im&oacute;vel à venda
						{% } else if (o.secao == 'Aluguel') { %}
							Im&oacute;vel para alugar
						{% } else if (o.secao == 'Lançamento')  { %}
							Im&oacute;vel Novo
						{% } %}
					</span>
				{% } %}
				{% if (o.bairro && o.cidade) { %}
					<span class="classified__content--location">
						<i class="icons-pin"></i>
						{%=o.bairro %}, {%=o.cidade%}, {%=o.estado%}
					</span>
				{% } else if (o.cidade){ %}
					<span class="classified__content--location">
						<i class="icons-pin"></i>
						{%=o.cidade%}, {%=o.estado%}
					</span>
				{% } %}
				<span class="classified__content--date" data-datareal="{%=o.data_insercao_h%}">
					Publicado em&nbsp;{%=o.data_insercao%}
				</span>
				<span class="classified__content--description hidden-xs">
					{% if (o.descricao) { %}
						{%=o.descricao.slice(0, 150)%}
						{% if (o.descricao.length > 150) { %}
							...
						{% } %}
					{% } else { %}
						Sem descrição
					{% } %}
				</span>
				<span class="classified__content--description visible-xs">
					{% if (o.descricao) { %}
						{%=o.descricao.slice(0, 70)%}
						{% if (o.descricao.length > 70) { %}
							...
						{% } %}
					{% } else { %}
						Sem descrição
					{% } %}
				</span>
				{% if (o.mini_descricao.length > 0){ %}
