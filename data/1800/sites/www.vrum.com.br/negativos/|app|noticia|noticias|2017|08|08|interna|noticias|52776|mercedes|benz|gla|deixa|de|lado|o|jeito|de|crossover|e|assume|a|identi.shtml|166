        function getTituloResumo() {
          var resp = $('.h1').html();
          $('.h3').each(function () {
            if ($(this).hasClass('margin-bottom-25')) { resp = resp + " " + $(this).html();}
          });
          return resp;
        }
        string2Search  = listSearchVeiculos['tags']  + "_" + getTituloResumo();
        url = "http://app.estadodeminas.vrum.com.br/apps,701,12/get-slog-fabricantes-modelos?palavras="+string2Search;
        /*$.getJSON2Script(url, function (data) {
          listSearchVeiculos['achados'] = data;
        });*/
