          url: url,
          type: 'GET',
          contentType: "application/json",
          dataType: 'jsonp', //'json',
          async:false,
          cache: true,
          jsonpCallback: 'get_slog_fabricantes_modelos',
          crossDomain: true
        }).done(function(data) {
