      <script type="text/javascript">
(function(selector){
    var widget = document.querySelector(selector);
    var container = widget.querySelector('[data-template="true"]').parentNode;
    var modelo = widget.querySelector('[data-template="true"]');
    var wrap = modelo.dataset['wrap'];
    var wrap_class = modelo.dataset['wrapClass'];
    var itens = [];

    var cidade = localStorage.busca_usuario?localStorage.busca_usuario.match(/{"name":"cidade","value":"(.*?)"}/):'';
    params = {'estado':getParceiro()[1]}
    params['palavras'] = [];
    params['palavras'].push(document.title);
    params['palavras'].push(document.querySelector('meta[name="description"]').content);
    params['palavras'].push(document.querySelector('meta[name="keywords"]').content);
    params['palavras'] = params['palavras'].join(' ');
    params['cidade'] = cidade?cidade[1]:'';
    params['limite'] = 6;

    $.ajax({
        url: 'http://app.estadodeminas.vrum.com.br/apps,701,19/get-recomendado-semantico',
        data: params,
        cache: true,
        dataType: 'jsonp',
        success: function (dados) {
            if(dados.ok){
                dados.ok.items.length && (widget.className = widget.className.replace(' hide',''));
                for(var i = 0; i < dados.ok.items.length; i++){
                    modelo.querySelector('a[data-template="link"]').href = dados.ok.items[i].urldestino;
                    modelo.querySelector('img[data-template="image"]').src = dados.ok.items[i].url_foto.pequena;
                    modelo.querySelector('[data-template="titulo"]').innerText = dados.ok.items[i].titulo;
                    modelo.querySelector('[data-template="ano"]').innerText = dados.ok.items[i].ano_fabricacao + '/' + dados.ok.items[i].ano_modelo;
                    modelo.querySelector('[data-template="preco"]').innerText = dados.ok.items[i].preco_formatado;
                    container.appendChild(modelo.cloneNode(true));

                    if(wrap && (i + 1) % wrap == 0){
                        var sep = modelo.cloneNode(false);
                        sep.className = wrap_class;
                        container.appendChild(sep);
                    }
                }
                if(dados.ok.urlbusca){
                    var h3 = jQuery(container).parent().find('h3');
                    jQuery(h3).html('<a href="' + dados.ok.urlbusca + '">' + (dados.ok.titulobusca || 'Veículos' ) +'</a>');
                    // container.firstChild.querySelector('a[data-template="link"]').href = dados.ok.urlbusca;
                    // container.firstChild.querySelector('[data-template="ano"]').innerText = 'a partir de';
                }
                modelo.remove();
            }else{
                widget.remove();
            }
        }
    });
})('.veiculos-relacionados')
</script> 
