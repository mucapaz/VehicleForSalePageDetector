  <script>
        $(document).ready(function () {
            $('#envio-proposta').on('show.bs.modal', function (e) {
                var dados = 'idveiculo=' + $(e.relatedTarget).data('id') + '&imagem=' + isMobile();
                var url = '/envio-proposta-modal.php'; //http://www.litoralcar.com.br/hplxbq
                $.ajax({
                    type: 'get',
                    url: url,
                    data: dados,
                    success: function (data) {
                        $('.fetched-data').html(data);
                    }
                });
            });
        });
    </script> 
