  <script>
        $(document).ready(function () {
            $("#leia-menos").on("click", function () {
                $("#retorno-texto-2").css("display", "none");
                $("#leia-menos").css("display", "none");
                $("#leia-mais").css("display", "block");
            });
            $("#leia-mais").click(function () {
                $("#retorno-texto-2").css("display", "block");
                $("#leia-menos").css("display", "block");
                $("#leia-mais").css("display", "none");
            });
        });

        $(document).ready(function () {
            $('#envio-proposta').on('show.bs.modal', function (e) {
                var dados = 'idveiculo=' + $(e.relatedTarget).data('id') + '&imagem=' + isMobile();
                var url = '/envio-proposta-modal.php'; //http://www.litoralcar.com.br/hplxbq
                $.ajax({
                    type: 'get',
                    url: url,
                    data: dados,
                    success: function (data) {
                        $('.fetched-data').html(data);
                    }
                });
            });
            $('#envio-proposta').on('hidden.bs.modal', function (e) {
                e.preventDefault();
                $('.fetched-data').html('');
            });
        });

    </script> 
