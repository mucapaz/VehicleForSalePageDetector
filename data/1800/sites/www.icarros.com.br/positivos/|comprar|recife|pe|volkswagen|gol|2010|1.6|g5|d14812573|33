                $(window).on('load', function(){
                    
                    if(!(typeof(_icUD) == 'undefined')){
                        if(!((typeof(_icUD.nome) == 'undefined')
                                && (typeof(_icUD.email) == 'undefined')
                                && (typeof(_icUD.cpf) == 'undefined')
                                && (typeof(_icUD.telefoneCompleto) == 'undefined'))
                        ) {
                            if(typeof(_icUD.cpf) == 'undefined') {
                                var rowLnks = $('.links-editar').closest('tr');
                                var rowToPlace = null;

                                $('#formProposta tr').each(function () {
                                    var cmp = $(this).find('input[type="text"].icr-input');
                                    if (cmp.length > 0 && !(cmp.is(":disabled"))) {
                                        rowToPlace = $(this);
                                        return false;
                                    }
                                });
                                if (rowToPlace != null) {
                                    rowLnks.detach();
                                    rowToPlace.before(rowLnks);
                                }
                            }
                            $('#tooltipDuvidas').show();
                            $('.links-editar').show();
                        }
                        if((typeof(_icUD.cpf) != 'undefined')){
                            var row = $('#row1');
                            var next = row.next();
                            if (next.is("tr")) {
                                row.detach();
                                next.after(row);
                            }
                            if(!($('tr.obter-cpf').is(':visible'))){
                                $('tr.obter-cpf').show();
                            }
                            $('.quersimular').prop('checked', true );
                        }
                        if(typeof(_icUD.email) == 'undefined'){
                            $('#newsletter').prop('disabled', false).closest('tr').show();
                        }
                    }
                });
                
                

                function analisarEmail(email, inputId, labelId, left) {
                    try {
                        // verifica email
                        var sugestao = analisarEmailRPC(email.value);
                        if(sugestao != null) {
                            var labelEmail;
                            var n = sugestao.split("@");
                            // se houver sugestão a exibe
                            var input = MM_findObj(inputId);
                            if(input != null) {
                                var div;
                                var label;

                                if(MM_findObj("divSugestaoEmail") == null) {
                                    div = document.createElement("div");
                                    div.id = "divSugestaoEmail";
                                    var parentDiv = input.parentNode;
                                    var n1 = parentDiv.parentNode;
                                    n1.parentNode.insertBefore(div, null);
                                } else {
                                    div = MM_findObj("divSugestaoEmail");
                                }

                                if(MM_findObj("labelSugestaoEmail") == null) {
                                    label = document.createElement("label");
                                    label.id = "labelSugestaoEmail";
                                } else {
                                    label = MM_findObj("labelSugestaoEmail");
                                }

                                //labelEmail = MM_findObj(labelId);
                                //if(labelEmail != null) {
                                //labelEmail.style.marginTop = "-22px";
                                //labelEmail.style.display = "block";
                                //}
                                div.style.height = "15px";
                                div.style.display = "block";
                                div.style.float = "left";
                                div.style.marginTop = "10px";
                                label.style.display = "block";
                                label.style.color = "#000";
                                label.style.whiteSpace = "nowrap";
                                var link = '<a onclick="javascript:corrigirEmail(\'' + inputId + '\', \'' + sugestao + '\', \'' + labelId + '\');" ' + ' style="color:#000000; text-decoration:underline;" href="javascript:void(0);" title="Clique aqui para corrigir seu email">';
                                label.innerHTML = "<img src='/comum/imagens/icone_atencao_p2.gif' style='vertical-align:bottom;' alt=''/> Você não quis dizer '" + link +  n[0] + "@<strong>" + n[1] + "</strong>'</a>?";

                                div.appendChild(label);
                            }
                        } else {
                            var cmpSugestaoEmail = MM_findObj("divSugestaoEmail");
                            if (cmpSugestaoEmail){
                                MM_findObj("divSugestaoEmail").style.display = "none";
                                labelEmail = MM_findObj(labelId);
                                if(labelEmail != null) {
                                    labelEmail.style.marginTop = "0";
                                }
                            }
                        }
                    } catch (e){}
                }

                function voltar(){
                    window.location.href = '/ache/listaanuncios.jsp?bid=0&sop=';
                }

                function showInformacoesAnunciante(){
                    _gaq.push(['_trackEvent', 'Detalhes', 'Informacoes Anunciante', 'Link']);
                    countStatistics(14812573, 1711113);
                    $('.toggle-dados-anunciante').slideToggle('slow', function(){});
                }

                $(document).ready(function(){

                    $('#formProposta').submit(function(event){

                        if(false){
                            if($('.whatsapp').is(':checked')){
                                var habilitar = validarWhatsApp;
                            }else{
                                var habilitar = validarNoPhoneWhatsApp;
                            }
                        }else{
                             var habilitar = null;
                        }

                        var ok = validarFormLead($(this),habilitar);

                        if(ok){
                            _gaq.push(['_trackEvent', 'Detalhes', 'Proposta', 'Botao']);
                            $(this).find('.icr-input:disabled').addClass('icr-disabled').prop('disabled', false);
                            var cpf = $(this).find('#cpf');
                            if(cpf != null && cpf.length > 0 && !cpf.is(':disabled')){
                                if(cpf.val().indexOf('X') > 0){
                                    cpf.val(cpf.attr('data-nav-value'));
                                }
                            }
                            return true;
                        }
                        event.preventDefault();
                    });

                    
                    $('.quersimular').on('change', function(){
                        if($(this).is(':checked')){
                            $('.quersimular').attr('checked',true);
                            $('tr.obter-cpf').show();
                            if($('.links-editar').is(':hidden') || (typeof(_icUD.cpf) == 'undefined')){
                                $('#formProposta').find('#cpf').prop('disabled', false);
                            }
                            
                        } else {
                            $('.quersimular').attr('checked',false);
                            
                                $('tr.obter-cpf').hide();
                                $('#formProposta').find('#cpf').prop('disabled', true);
                            
                            
                        }
                    });
                    

                    
                        initSwiper();
                    

                    showStickyHeader('ctdoTopo','true', function(){
                        var canSticky = $('.can-sticky');
                        $.each(canSticky, function(){
                            if($(this).hasClass('sticky-out')){
                                $(this).removeClass('sticky-out');
                            } else {
                                $(this).addClass('sticky-out');
                            }
                        });
                    });

                    $('#cardProposta').sticky({
                        'stopOnElement': $('.cross-modelo')
                    });

                    

                    

                    
                });

            </script> 
