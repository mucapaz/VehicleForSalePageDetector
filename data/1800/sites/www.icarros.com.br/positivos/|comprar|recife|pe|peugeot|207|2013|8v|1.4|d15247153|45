                $(window).on('load', function(){
                    
                        $('#cpf').prop('disabled', (typeof(_icUD) != 'undefined' && typeof(_icUD.cpf) != 'undefined'));
                        $('tr.obter-cpf').show();
                        $('.quersimular').prop('checked', true );
                    
                    if(!(typeof(_icUD) == 'undefined')){
                        if(!((typeof(_icUD.nome) == 'undefined')
                                && (typeof(_icUD.email) == 'undefined')
                                && (typeof(_icUD.cpf) == 'undefined')
                                && (typeof(_icUD.telefoneCompleto) == 'undefined'))
                        ) {
                            if(typeof(_icUD.cpf) == 'undefined') {
                                var rowLnks = $('.links-editar').closest('tr');
                                var rowToPlace = null;

                                $('#formProposta tr').each(function () {
                                    var cmp = $(this).find('input[type="text"].icr-input');
                                    if (cmp.length > 0 && !(cmp.is(":disabled"))) {
                                        rowToPlace = $(this);
                                        return false;
                                    }
                                });
                                if (rowToPlace != null) {
                                    rowLnks.detach();
                                    rowToPlace.before(rowLnks);
                                }
                            }
                            $('#tooltipDuvidas').show();
                            $('.links-editar').show();
                        }
                        if((typeof(_icUD.cpf) != 'undefined')){
                            var row = $('#row1');
                            var next = row.next();
                            if (next.is("tr")) {
                                row.detach();
                                next.after(row);
                            }
                            if(!($('tr.obter-cpf').is(':visible'))){
                                $('tr.obter-cpf').show();
                            }
                            $('.quersimular').prop('checked', true );
                        }
                        if(typeof(_icUD.email) == 'undefined'){
                            $('#newsletter').prop('disabled', false).closest('tr').show();
                        }
                    }
                });
                
                
                    var map, latlng, endereco, myOptions;
                    function loadMap() {
                        if(map == null || map == 'undefined') {
                            latlng = new google.maps.LatLng(-8.055465, -34.907055);
                            geocoder = new google.maps.Geocoder();
                            if(latlng.lat() == 0 || latlng.lng() == 0){
                                geocoder.geocode({'address': 'Av. Visconde de Albuquerque 300, Madalena Recife PE'}, function(results, status) {
                                    if (status == google.maps.GeocoderStatus.OK) {
                                        latlng = results[0].geometry.location;
                                    }
                                });
                            }
                            myOptions = {zoom: 15, center: latlng, keyboardShortcuts:false, scrollwheel:false, navigationControl: true, mapTypeControl: false, mapTypeId: google.maps.MapTypeId.ROADMAP};
                            map = new google.maps.Map(MM_findObj("gMaps"), myOptions);
                            geocoder.geocode({
                                'latLng': latlng
                            }, function(results, status) {
                                if (status == google.maps.GeocoderStatus.OK) {
                                    map.setCenter(results[0].geometry.location);
                                    var marker = new google.maps.Marker({
                                        position:results[0].geometry.location,
                                        'map': map,
                                        'title' : 'ASTRAL VEICULOS'
                                    });
                                    var infowindow = new google.maps.InfoWindow();
                                    google.maps.event.addListener(marker, 'click', function() {
                                        infowindow.setContent('<table cellpadding="5" cellspacing="5" border="0"><tbody><tr><td style="vertical-align:top;" width="200"><h1><strong>ASTRAL VEICULOS</strong></h1><h2>Av. Visconde de Albuquerque 300 - Recife / PE</h2></td><td valign="top" width="90"><img width="90" src="http://img0.icarros.com/dbimg/imganunciante/1/123937_2" /></td></tr></tbody></table>');
                                        infowindow.open(map, this);
                                    });
                                }
                            });
                            $('#dtLocalizacao').slideDown('slow');
                        }

                    }
                    function tracarRota(){
                        var localizacaoUsuario = MM_findObj("endereco_usuario").value;

                        if (localizacaoUsuario == "" || localizacaoUsuario == 'undefined') {
                            alert("Por favor, digite um endereço.");
                            return;
                        }

                        var directionsDisplay = new google.maps.DirectionsRenderer();
                        directionsDisplay.setMap(map);
                        directionsDisplay.setPanel(MM_findObj("gDirections"));

                        var request = {
                            origin:  localizacaoUsuario + " - Brasil",
                            destination: latlng,
                            travelMode: google.maps.DirectionsTravelMode.DRIVING,
                            region: 'BR'
                        };

                        var directionsService = new google.maps.DirectionsService();
                        directionsService.route(request, function(result, status) {
                            if (status == google.maps.DirectionsStatus.OK) {
                                MM_findObj("gDirections").style.display = 'block';
                                directionsDisplay.setDirections(result);
                            }
                        });
                    }
                

                function analisarEmail(email, inputId, labelId, left) {
                    try {
                        // verifica email
                        var sugestao = analisarEmailRPC(email.value);
                        if(sugestao != null) {
                            var labelEmail;
                            var n = sugestao.split("@");
                            // se houver sugestão a exibe
                            var input = MM_findObj(inputId);
                            if(input != null) {
                                var div;
                                var label;

                                if(MM_findObj("divSugestaoEmail") == null) {
                                    div = document.createElement("div");
                                    div.id = "divSugestaoEmail";
                                    var parentDiv = input.parentNode;
                                    var n1 = parentDiv.parentNode;
                                    n1.parentNode.insertBefore(div, null);
                                } else {
                                    div = MM_findObj("divSugestaoEmail");
                                }

                                if(MM_findObj("labelSugestaoEmail") == null) {
                                    label = document.createElement("label");
                                    label.id = "labelSugestaoEmail";
                                } else {
                                    label = MM_findObj("labelSugestaoEmail");
                                }

                                //labelEmail = MM_findObj(labelId);
                                //if(labelEmail != null) {
                                //labelEmail.style.marginTop = "-22px";
                                //labelEmail.style.display = "block";
                                //}
                                div.style.height = "15px";
                                div.style.display = "block";
                                div.style.float = "left";
                                div.style.marginTop = "10px";
                                label.style.display = "block";
                                label.style.color = "#000";
                                label.style.whiteSpace = "nowrap";
                                var link = '<a onclick="javascript:corrigirEmail(\'' + inputId + '\', \'' + sugestao + '\', \'' + labelId + '\');" ' + ' style="color:#000000; text-decoration:underline;" href="javascript:void(0);" title="Clique aqui para corrigir seu email">';
                                label.innerHTML = "<img src='/comum/imagens/icone_atencao_p2.gif' style='vertical-align:bottom;' alt=''/> Você não quis dizer '" + link +  n[0] + "@<strong>" + n[1] + "</strong>'</a>?";

                                div.appendChild(label);
                            }
                        } else {
                            var cmpSugestaoEmail = MM_findObj("divSugestaoEmail");
                            if (cmpSugestaoEmail){
                                MM_findObj("divSugestaoEmail").style.display = "none";
                                labelEmail = MM_findObj(labelId);
                                if(labelEmail != null) {
                                    labelEmail.style.marginTop = "0";
                                }
                            }
                        }
                    } catch (e){}
                }

                function voltar(){
                    window.location.href = '/ache/listaanuncios.jsp?bid=0&sop=';
                }

                function showInformacoesAnunciante(){
                    _gaq.push(['_trackEvent', 'Detalhes', 'Informacoes Anunciante', 'Link']);
                    countStatistics(15247153, 123937);
                    $('.toggle-dados-anunciante').slideToggle('slow', function(){});
                }

                $(document).ready(function(){

                    $('#formProposta').submit(function(event){

                        if(false){
                            if($('.whatsapp').is(':checked')){
                                var habilitar = validarWhatsApp;
                            }else{
                                var habilitar = validarNoPhoneWhatsApp;
                            }
                        }else{
                             var habilitar = null;
                        }

                        var ok = validarFormLead($(this),habilitar);

                        if(ok){
                            _gaq.push(['_trackEvent', 'Detalhes', 'Proposta', 'Botao']);
                            $(this).find('.icr-input:disabled').addClass('icr-disabled').prop('disabled', false);
                            var cpf = $(this).find('#cpf');
                            if(cpf != null && cpf.length > 0 && !cpf.is(':disabled')){
                                if(cpf.val().indexOf('X') > 0){
                                    cpf.val(cpf.attr('data-nav-value'));
                                }
                            }
                            return true;
                        }
                        event.preventDefault();
                    });

                    
                    $('.quersimular').on('change', function(){
                        if($(this).is(':checked')){
                            $('.quersimular').attr('checked',true);
                            $('tr.obter-cpf').show();
                            if($('.links-editar').is(':hidden') || (typeof(_icUD.cpf) == 'undefined')){
                                $('#formProposta').find('#cpf').prop('disabled', false);
                            }
                            
                                $('span.observacao').closest('tr').show();
                                $('#btnEnviar span').text('simular financiamento');
                                $('input[type=hidden][name="showsimulador"]').val(1);
                            
                        } else {
                            $('.quersimular').attr('checked',false);
                            
                                $('tr.obter-cpf').hide();
                                $('#formProposta').find('#cpf').prop('disabled', true);
                            
                            
                                $('span.observacao').closest('tr').hide();
                                $('#btnEnviar span').text('enviar proposta');
                                $('input[type=hidden][name="showsimulador"]').val(0);
                            
                        }
                    });
                    

                    
                        initSwiper();
                    

                    showStickyHeader('ctdoTopo','true', function(){
                        var canSticky = $('.can-sticky');
                        $.each(canSticky, function(){
                            if($(this).hasClass('sticky-out')){
                                $(this).removeClass('sticky-out');
                            } else {
                                $(this).addClass('sticky-out');
                            }
                        });
                    });

                    $('#cardProposta').sticky({
                        'stopOnElement': $('.cross-modelo')
                    });

                    

                    
                    $('#fotoAnunciante').click(function () {
                        if( $(this).attr('id') == 'fotoAnunciante'){
                            $('.fotoAnunciante').hide();
                            $('.videoAnunciante').show();
                            document.getElementById('playbuttonAnunciante').className = 'videoAnunciante';
                            document.getElementById('setaPlay').className = 'icones icone-back iconeplay';
                        }
                    });
                    

                    
                });

            </script> 
