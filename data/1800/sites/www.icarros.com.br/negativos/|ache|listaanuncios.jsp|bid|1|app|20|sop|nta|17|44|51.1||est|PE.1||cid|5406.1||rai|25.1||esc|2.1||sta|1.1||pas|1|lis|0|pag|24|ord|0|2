            $(document).ready(function(){
                doLoginCheck("#infousuario");
                $('input, textarea').placeholder();
                $('body').on('click', function(){
                    if($('#eScreen').length > 0){f_dialogClose();}
                    if($('.ouibounce-modal').length > 0){$('.ouibounce-modal').hide()}
                });
                if($('.ouibounce-modal').length > 0){
                    var obM = $('.ouibounce-modal');
                    var ckN = obM.attr('icr-ckn');
                    var ckE = obM.attr('icr-cke');
                    $('.ouibounce-modal .modal').on('click',function(e){e.stopPropagation();});
                    ouibounce(obM[0], {
                        sitewide: true,
                        cookieDomain : '.icarros.com.br',
                        cookieExpire : (ckE > 0 ? parseInt(ckN) : 5),
                        cookieName : ckN,
                        delay: 200, //delay (ms) entre o cursor sair do viewport e o modal aparecer
                        timer: 5000 //tempo (ms) depois do load que precisa passar para que o ouibounce seja ativado
                    });
                }
                $("img.imglazy").lazy({
                    threshold:0,
                    visibleOnly: true,
                    bind:"event"
                });
                $(".lazycontainer").hover( function() {
                    var forDiv=$(this).attr('for');
                    var searchInto=forDiv!=null ? $("#"+forDiv) : $(this);
                    searchInto.find("img.imglazy").lazy({
                        bind: "event",
                        delay: 0
                    });
                });
            });

