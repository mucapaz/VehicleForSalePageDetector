  <script type="text/javascript">

        $(document).ready(function(){
            showStickyHeader('ctdoTitle');
            showBotaoEnviarProposta('','','');
            var from = '';
            if(from != null && from == 'ctg'){
                setTimeout(function(){
                    $('.alert-modal').show().animate({bottom: ['5',  'easeInBack']}, 1000)
                });
            }
            var hash = window.location.hash;
            if(hash != null && hash.length > 0 && hash.indexOf('rfae') > 0){
                $('.breadcrumbs').after('<div class="icr-msg-info"><i class="icones icone-warning"></i>O anúncio que você acessou não está mais disponível.<i class="direita icones icone-fechar-1 close" onclick="$(\'.icr-msg-info\').fadeOut(1000);"></i></div>');
                $('.icr-msg-info').fadeIn(1000);
            }
        });

        pushAfterLoginCustomFunctions('showBannerPCI', function (){
            if(typeof pciCheck != 'undefined' && pciCheck.userHasPCI && typeof _icDNVG != 'undefined' && (_icDNVG.icP == 198 || _icDNVG.icP == 29)){
                try {
                    $('.titulo').before('<div class="clearfix banner-itau">' +
                            '<img width="60" src="http://img1.icarros.com/comum/imagens/logo_itau_cross.png" alt="Itaú" />' +
                            '<h6 style="' + (!pciCheck.userHasPA ? 'padding-top:18px;' : '') + '">' + (pciCheck.userHasPA ? 'Aproveite seu crédito pré analisado* no Itaú e escolha seu novo carro!' : 'Confira as ofertas baseadas na simulação de financiamento.') + '</h6>' +
                            (pciCheck.userHasPA ? '<p>Confira as ofertas baseadas na simulação de financiamento.</p> ' : '') + '</div>'
                    );
                    if(pciCheck.userHasPA){
                        $('.cross').after('<p style="font-size:0.85em;">*Para que o contrato seja concretizado, é necessária a aprovação da proposta de crédito, confirmação dos dados informados, aprovação do veículo escolhido pela instituição financeira, bem como apresentação da documentação por esta exigida.</p>');
                    }
                } catch (e) {}
            }
        });

        function showBotaoEnviarProposta(modeloId, marcaId, kmMaxima) {
            try {
                if ((modeloId!=null) && (modeloId!="") &&
                   ((kmMaxima==null)||(kmMaxima==0))  ) {
                    getGruposRegionaisDisponivelModelo(modeloId,function(data) {
                        if(data && data.length>0) {
                            var gruposRegionais = data.toString();
                            $('#ctdoTitle').find('#divLead0km').html('');
                            $('#ctdoTitle').find('#divLead0km').prepend("<ul id='lstlead0km' class='listahorizontal' style='float: right;'><li id='btn-proposta'></li></ul>");
                            $('#ctdoTitle').find('#divLead0km').find('#btn-proposta').append("<a href=\"javascript:lnkProposta0km(0,'"+marcaId+"','"+modeloId+"',0,'"+gruposRegionais+"',false);\" class='icr-btn icr-btn-redondo icr-btn-s icr-btn-laranja' style='padding:7px;' onclick='pushEventEnviarProposta();'>enviar proposta 0km</a>");

                            $('#ctdoTitle').find('#feiraoStick').addClass("feirao-leadkm-icarros-sticky");
                            $('#ctdoTitle').find('#lnkOfferFeirao').hide();
                            try {
                                pushAfterLoginCustomFunctions('visaogeral-phone-botao-enviarproposta', function () {
                                    registerGAEvents('Comprar','Lista','Views Botão Enviar Proposta 0km');
                                });
                            } catch (e) {  }
                        }else{
                            $('#ctdoTitle').find('#lnkOfferFeirao').show();
                        }
                    })
                }
            } catch (e){ console.log(e);}
        }

        function pushEventEnviarProposta(){
            _gaq.push(['_trackEvent', 'Comprar', 'Lista', 'Clique Botão Enviar Proposta 0km'])
        }
