  <script type="text/javascript">
        var icc_requestedComponents = 'select,inputtext,slider'.split(",");
        var icc_registeredComponents = jQuery.Callbacks( "memory" );
        function iccAddComponentToInit(component,fn) {
            if($.inArray(component, icc_requestedComponents)>-1) {
                icc_registeredComponents.add(fn);
            }
        }
        function initIccComponents() {
            icc_registeredComponents.fire();
        }
        $(document).ready(function() {
            initIccComponents();
        });
