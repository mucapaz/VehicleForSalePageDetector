   <script type="text/javascript">

                function refreshSlideShow(){
                    var imgs = $('.icc_galeria_thumbs_container').find('img');
                    var imgIdx = imgs.filter('.current').index();
                    var imgCount = $('.icc_galeria_imgcount > span');
                    var imgTD = $('.icc_texto_descritivo > span');
                    var imgAtual = $(imgs.get(imgIdx));
                    if(imgIdx == 0){
                        imgCount.html('');
                    } else {
                        imgCount.html(imgIdx + ' | ' +  (imgs.length - 1));
                    }
                    imgTD.html(imgAtual.attr('data-textodescritivo'));
                }
                function refreshPublicidade() {
                    if (_icDNVG && icDNVG.icT.name == 'ICARROS') {
                        var postscribe = true;
                        if ($('#ad_carrosp1').text().length > 1) {
                            postscribe = false;
                            $('#ad_carrosp1').html('');
                        }

                        $('#ad_carros0').html('');
                        $('#ad_carros1').html('');

                        if (postscribe) {
                            addLoadEvent(get_ads());
                        } else {
                            addLoadEvent(place_ads());
                            addResizeEvent(place_ads());
                        }
                    }
                }
            </script> 
