  <!-- BEGIN TAG MANAGER --> 
  <script type="text/javascript">
	var mobileSources = ['mobile', 'msite', 'ios', 'android'];

	function isMobile() {
		var source = getSource();

		if (mobileSources.indexOf(source) != -1) {
			return true;
		}
		return false;
	}

	function getSource() {
		var userAgent = navigator.userAgent.toLowerCase();
		var source = "";

		if (window.bna_vars && bna_vars.src) {
			if (bna_vars.src == 'w') {
				source = 'web';
			} else if (bna_vars.src == 'm') {
				source = 'msite';
			}
		} else {
			source = ''.toLowerCase();
			if (source == 'mobile') {
				source = 'msite'
			}
		}

		var resultMatch = userAgent.match(/olx-source\/(\w+);/);
		if (resultMatch) {
			source = resultMatch[1];
		}

		if (mobileSources.indexOf(source) == -1 && source != 'web') {
			source = 'web';
		}

		return source;
	}

	dataLayer = function() {
		var initialDatalayer = {
			'config': {
