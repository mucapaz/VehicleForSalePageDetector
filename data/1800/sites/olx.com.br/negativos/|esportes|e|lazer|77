  <script type="text/javascript">
	var mobileSources = ['mobile', 'msite', 'ios', 'android'];

	function isMobile() {
		var source = getSource();

		if (mobileSources.indexOf(source) != -1) {
			return true;
		}
		return false;
	}

	function getSource() {
		var userAgent = navigator.userAgent.toLowerCase();
		var source = "";

		if (window.bna_vars && bna_vars.src) {
			if (bna_vars.src == 'w') {
				source = 'web';
			} else if (bna_vars.src == 'm') {
				source = 'msite';
			}
		} else {
			source = ''.toLowerCase();
			if (source == 'mobile') {
				source = 'msite'
			}
		}

		var resultMatch = userAgent.match(/olx-source\/(\w+);/);
		if (resultMatch) {
			source = resultMatch[1];
		}

		if (mobileSources.indexOf(source) == -1 && source != 'web') {
			source = 'web';
		}

		return source;
	}

	dataLayer = function() {
		var initialDatalayer = {
			'config': {
				'lurkerURL': '//tracking.olx.com.br/track'
			},
			'site': {
				'isMobile': isMobile(),
				'source': getSource()
			},
			'page': {
				'pageType': 'listing',
				'detail': {
					
					'category_id': '6000',
					
					
					
					
				},
				
					'searchDetail': {
						
							'mainCategory': 'Esportes e lazer',
							'mainCategoryID': '6000',
						
						
						
						'subRegion': '',
						'subRegionID': '',
						'filters': '',
					},
				
			},
			'session': {
				'user': {
					'userID': null,
					'loginType': null
				}
			},

			'pageType': 'Result_page',
			'abtestingEnable' : '1',

			

			// Listing information
			
			'listingCategory': '6000',
			

			// Ad information
			
			
			
			
			
			
			'listId': '',

			//Account Information
			
			'loggedUser':'0',
			
			'referrer': '',

			//User Information
			
			
		};

		if (self.adParams) {
			for (key in adParams) {
				var page = initialDatalayer.page;
				page.detail[key] = adParams[key];
				if (page.adDetail) {
					page.adDetail[key] = adParams[key];
				}
			}
		}
		return [initialDatalayer];
	}();
