    <!-- OLX For Shopping<br> --> 
    <!-- Isn't iOS...<br> --> 
    <div id="module_olx_shopping"></div> 
    <script>
    var OLXShopping = function (options) {
      this.container = document.getElementById(options.container);
      this.placement = options.placement;
      this.url       = '//t.dynad.net';

      var defaultParams = {
        dc      : this.placement,
        srctype : 'ifrm'
      }

      this.params = $.extend(defaultParams, options.params);

      options.print && this.print();
    }

    OLXShopping.prototype.getQueryParams = function () {
      var qp = [];
      for (var key in this.params) {
        qp.push(key + '=' + this.params[key]);
      }
      return '?' + qp.join(';');
    }

    OLXShopping.prototype.hide = function () {
      this.container.style.height = 0;
      this.container.style.overflow = 'hidden';
    }

    OLXShopping.prototype.print = function () {
      var queryparams = this.getQueryParams();
      var markup = [];
      markup.push(
        '<iframe src="' + this.url + '/script/' + queryparams + '" marginwidth="0" height="330" marginheight="0" align="top" scrolling="Yes" bordercolor="#000000" frameborder="0" hspace="0" vspace="0">',
        '  <script language="JavaScript1.1" src="' + this.url + '/script/?dc=' + this.placement + ';"></scr' + 'ipt>',
        '  <noscript>',
        '    <a href="https://' + this.url + '/c/?dc=' + this.placement + ';c=0;srctype=bkp" target="_blank">',
        '      <img src="//t.dynad.net/serve/?dc=' + this.placement + ';srctype=bkp" border="0" alt="" height="330" />',
        '    </a>',
        '  </noscript>',
        '</iframe>'
      );
      this.container.classList.add('OLXShopping--visible');
      this.container.innerHTML = markup.join('');
    }
  
    
    
    // <pre>shopping_cg: 7000</pre>
    // <pre>shopping_var: listing_category</pre>
    // <pre>%listing_category: 7000</pre>
    // <pre>parent_cg: </pre>
    // <pre>shopping_category: 7000</pre>
    // <pre>parent_category: </pre>
    // <pre>parent: </pre>
   // <pre>cg: 7000</pre>
   // <pre>category: 7060</pre>
   // <pre>appl: li</pre>
  

  
  
      // <!-- Channel is available...<br> -->
      
        // <!-- Page is valid...<br> -->
        
          // <!-- Has PubId...<br> -->
          // <!-- Initiating OLX For Shopping:<br> -->

          
          
          
          
            var query = '';

          
            query = ''.trim() || ' empregos e negócios'.trim();
          

            var shopping = new OLXShopping({
              container : 'module_olx_shopping',
              placement : 5550003352,
              print: true,
              params    : {
                source          : '',
                uf              : '',
                ddd             : '',
                search_kw       : S(query).latinise().s,
                category        : '7000',
                afsh_channel_id : 'shopping_jobs',
                afsh_pub_id     : 'partner-vert-pla-olx-cat'
              }
            });
        
      
    
  
    window.addEventListener('message', function (event) {
      if (event.data == 'PUB_AFShAdLoadedCallback') {
        // console.log('loaded afsh');
      }
      if (event.data == 'PUB_AFShNoAdLoadedCallback') {
        // console.log('no ads on afsh');
        shopping.hide();
      }
      if (event.data == 'PUB_AFShNoQueryErrorCallback') {
        // console.log('no ads on afsh');
        shopping.hide();
      }
      if (event.data == 'PUB_OLXShoppingNoAdLoadedCallback') {
        // console.log('no ads on olx shopping');
        shopping.hide();
      }
    });
  </script> 
