(function() {
    'use strict';

    window.OLX_HELPER = window.OLX_HELPER || {};

    window.OLX_HELPER = (function() {
        var Helper = function() {};

        if (document.domain.match(/^m\./i)) {
            Helper.platform = "mobile";
        } else {
            Helper.platform = "web";
        }
        Helper.appl = "li";
        return Helper;
    })();
})();
