  <script type="text/javascript">
$('#main-ad-list').bind('DOMNodeInserted', function(e) {
    timerConv = setTimeout(convChar, 1000, e.target);
    var convChar = function(target) {
      var attr = $(target).attr('id');
      if(typeof attr !== typeof undefined && attr !== false && attr.indexOf('gemini') > -1) {
        $(target).find('.OLXad-list-title, .detail-specific').each(function(){
            try {
            $(this).text((decodeURIComponent(escape($(this).text()))))
            } catch(e) {
            clearTimeout(timerConv);
            }
        });
      }
    };
    convChar(e.target);
});
