		googletag.pubads().addEventListener('slotRenderEnded', function(event) {
			var code = event.slot.getSlotElementId();
			var format = event.size.join('x');

			trackableAdUnits[code].end_load = new Date().getTime();
			trackableAdUnits[code].creative_id = event.creativeId;
			trackableAdUnits[code].line_item = event.lineItemId;
			trackableAdUnits[code].format = format;
			trackableAdUnits[code].rendered = true;

			OLX.Advertisement.Dfp.setStateSlot(code, event.creativeId, event.lineItemId, event.isEmpty, format);

			var allUnitsAreRendered = true;
			for (var unitCode in trackableAdUnits) {
				if (!trackableAdUnits[unitCode].rendered) {
					allUnitsAreRendered = false;
				}
			}
			if (allUnitsAreRendered) {
				sendTrackableAdUnitsToLurker();
			}

			if (code == 'web_listing_banner_top' && event.size[1] == 250) {
				setTimeout(function() {
					var billboard_disabled_time = parseInt('400' || 0);
					// é billboard
					jQuery('#' + code)
						.addClass('disabled-billboard')
						.css('height', 250);
					setTimeout(function() {
						jQuery('#' + code).removeClass('disabled-billboard')
					}, 600 + billboard_disabled_time);
				});
			}

		});

		var trackAdUnitsTimeout = setTimeout(function() {
			sendTrackableAdUnitsToLurker();
		}, trackableAdUnitsTimeout);

		var sendTrackableAdUnitsToLurker = function() {
			if (hasTrackedAllAdUnits) {
				return;
			}
			clearTimeout(trackAdUnitsTimeout);
			hasTrackedAllAdUnits = true;
			Lurker.send({
				event_type: 'impression',
				object_detail: 'advertising',
				adv_data: trackableAdUnits
			});
			return null;
		}

		window.onbeforeunload = sendTrackableAdUnitsToLurker;

		if(window.crtg_content != null) {
			if(crtg_content.match('.*criteo600=1.*')) {
				googletag.pubads().setTargeting("criteo600", "1");
			}
		}

		if (window.bn_cookies) {
			var testABOnlineSalesBanner = bn_cookies.read("TestAB_OnlineSalesBanner");
			var testABPub734 = "PUB-734-"+(bn_cookies.read("TestAB_PUB734") || "ORIGINAL");

			googletag.pubads().setTargeting("onlie_sales_banner", testABOnlineSalesBanner == 'A' ? "true" : "false");
			googletag.pubads().setTargeting("ABTest", testABPub734);
		}

		googletag.pubads().enableSingleRequest();
	});


