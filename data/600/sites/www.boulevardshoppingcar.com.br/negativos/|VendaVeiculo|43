  <script type="text/javascript">
        $(this.document).ready(function () {
            $("#buttonEmailContato").click(function (e) {
                $.ajaxSubmitJsonResponse("#formContato");
                //$("#formContato").validate({
                //    submitHandler: function (form) {
                //        $.ajaxSubmitJsonResponse(form);
                //        return false;
                //    }
                //});
            });

            loadSelect("/Home/ObterTodasMarcasPorTipo/1", null, "#Marca", '0', function () {
                $("#Marca").selectpicker("refresh");
                $.unblockUI();
            });

            $("#Marca").change(function () {
                showBlockUI();
                loadSelect("/Home/ObterTodosModelosPorMarca/" + $(this).val(), null, "#Modelo", '0', function () {
                    $("#Modelo").selectpicker("refresh");
                    $.unblockUI();
                });
            });

            setTimeout(function () {
                $("#Marca").selectpicker("refresh");
            }, 5000);
        });

    </script> 
  <script type="text/javascript">
        function removerFoto(uniqueID) {
            $('#' + uniqueID).hide();

            if ($('#fotosDel').val() !== "")
                $('#fotosDel').val($('#fotosDel').val() + "," + uniqueID);
            else
                $('#fotosDel').val(uniqueID);
        }

        $(function () {
            'use strict';
            var url = '/Upload/UploadHandler.ashx';
            $('#fileupload').fileupload({
                url: url,
                formData: { folderTemp: '530f2cd5-5a2d-41dd-9dda-370cd7da22c4' },
                dataType: 'json',
                sequentialUploads: true,
                done: function (e, data) {
                    $.each(data.files, function (index, file) {
                        $('#folderTemp').val('530f2cd5-5a2d-41dd-9dda-370cd7da22c4');
                        var foto = "http://www.autocerto.com/fotosTemp/" + $('#folderTemp').val() + "/" + file.name;
                        var fileName = "'" + file.name.replace(".", "") + "'";

                        $('#imagens ul').append('<li id=' + fileName + ' class="ui-state-default"><img src="' + foto + '" style="width:130px!important; height:118px!important" alt=""/><a href="#" onclick="removerFoto(' + fileName + ');" class="icon" title="Excluir Foto" style="vertical-align: top; color: black; font-size: 12px;"><i class="fa fa-trash" style="vertical-align:top; margin-top: 3px; font-size: 12px;"></i> Excluir Foto</a></li>');
                        $('#progress .progress-bar').css('width', '0%');
                    });
                },
                progressall: function (e, data) {
                    var progress = parseInt(data.loaded / data.total * 100, 10);

                    $('#progressInfo').html(progress + " % | " + parseFloat(data.loaded / 1024).toFixed(2) + " KB / " + parseFloat(data.total / 1024).toFixed(2) + " KB");
                    $('#progress .progress-bar').css(
                        'width',
                        progress + '%'
                    );
                }
            });
        });
    </script>  
