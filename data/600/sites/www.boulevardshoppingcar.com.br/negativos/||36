        $(this.document).ready(function () {

            loadSelect("/Home/ObterMarcasPorTipo/1", null, "#Marca", '0', function () { });

            $("#Marca").change(function () {
                loadSelect("/Home/ObterModelosPorMarca/" + $(this).val(), null, "#Modelo", '0', function () { });
            });

