   <input id="UserIdCh" name="UserIdCh" type="hidden" value="0"> 
   <input id="UserNameCh" name="UserNameCh" type="hidden" value=""> 
   <input id="UserEmailCh" name="UserEmailCh" type="hidden" value=""> 
   <input id="UserAllowMailCh" name="UserAllowMailCh" type="hidden" value="False"> 
   <input id="UserStateCh" name="UserStateCh" type="hidden" value=""> 
   <input id="UserCityCh" name="UserCityCh" type="hidden" value=""> 
   <script>

    function getCookie(chave) {
        var ChaveValor = document.cookie.match('(^|;) ?' + chave + '=([^;]*)(;|$)');
        return ChaveValor ? ChaveValor[2] : null;
    }

    var userState = getCookie('GeoUF');
    var userCity = getCookie('GeoCity');

    var userId = $("#UserIdCh").val();
    if (userId !== "0" && userId !== "") {
        var userName = $("#UserNameCh").val();
        var userEmail = $("#UserEmailCh").val();
        var userAllowMail = $("#UserAllowMailCh").val().toLowerCase() === 'true';

        if ($("#UserStateCh").val() != '') {
            userState = $("#UserStateCh").val();
            userCity = $("#UserCityCh").val();
        }

        if (userState != '' && userState != null) {
            window.chaordic_meta = {
                "page": "other",
                "tags": [userCity, userState]
            }
        }
        else {
            window.chaordic_meta = {
                "page": "other"
            }
        }

        window.chaordic_meta.user = {
            "id": userId,
            "name": userName,
            "email": userEmail,
            "allow_mail_marketing": userAllowMail
        }
    } else {

        if (userState != null) {
            window.chaordic_meta = {
                "page": "other",
                "tags": [userCity, userState]
            }
        }
        else {
            window.chaordic_meta = {
                "page": "other"
            }
        }
    }
</script> 
