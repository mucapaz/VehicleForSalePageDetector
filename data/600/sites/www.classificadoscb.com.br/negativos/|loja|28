  <script type="text/javascript">
		var limit_busca = 20;
		function carregar_lojas(dados, callback){
			var data = {};
			if (jQuery('#id').val()){
				data.id = jQuery('#id').val();
				data.tipoloja = jQuery('#tipoloja').val();
				data.uri = jQuery('#uri').val();
			}
			if (dados){
				jQuery.extend(data, dados);
			}

			jQuery.ajax({
				url: '/loja_busca',
				type: 'GET',
				data: data,
				dataType: 'json'
			})
			.done(function(data) {
				jQuery('#box_lojas').html(tmpl("tmpl_box_loja", data));
				paginacao({
					'num_page': data.num_page,
					'max_page': data.max_page,
					'limit': data.limit,
					'offset': data.offset,
				});
			})
			.fail(function() {
				console.log("error");
			});
		}
		jQuery(document).ready(function($) {
			carregar_lojas();
			jQuery('.js-paginacao-next').on('click', function(){
				var pagina = parseInt(jQuery('#edt-page').val());
				var dados = {
					'apartirde': ( pagina * limit_busca )
				};
				carregar_lojas(dados);
			});

			jQuery('.js-paginacao-prev').on('click', function(){
				var pagina = parseInt(jQuery('#edt-page').val());
				var dados = {
					'apartirde': ( (pagina - 1) * limit_busca)  - limit_busca
				};
				carregar_lojas(dados);
			});

			jQuery('#btn_paginacao').on('click', function(){
				var pagina = parseInt(jQuery('#edt-page').val());
				var dados = {
					'apartirde': (Math.ceil( (pagina - 1) * limit_busca))
				};
				carregar_lojas(dados);
			});
			jQuery('.js-inpBusca').on('keypress', function(e){
				if(e.which == 13) {
					e.preventDefault();
					jQuery('body').addClass('load proccess');
					jQuery.ajax({
						url: '/loja_busca',
						type: 'GET',
						data: {
							'id': jQuery('#id').val(),
							'por': jQuery('.js-inpBusca').val(),
							'tipoloja': jQuery('#tipoloja').val()
						},
						dataType: 'json'
					})
					.done(function(data) {
						jQuery('#box_lojas').html(tmpl("tmpl_box_loja", data));
						paginacao({
							'num_page': data.num_page,
							'max_page': data.max_page,
							'limit': data.limit,
							'offset': data.offset,
						});
						jQuery('body').removeClass('load proccess');
					})
					.fail(function(data) {
						utils.info(data.responseJSON.msg, 3000, 'error');
						jQuery('body').removeClass('load proccess');
					});
				}
			});
		});
	</script>   
 </body>
