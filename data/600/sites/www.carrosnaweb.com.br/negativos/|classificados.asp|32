  <script language="JavaScript">
	function MM_openBrWindow(theURL,winName,features) 
	{ 
		window.open(theURL,winName,features);
	}
	
function checa()
{
	var error_string = "";
	var PrimeiroErro = "";
	if ((window.document.form1.lojas.checked == false)&&(window.document.form1.particulares.checked == false))
	{
		error_string += "Favor informar se prefere buscar de lojas e/ou de particulares.\n";
		PrimeiroErro = "particulares";
	}
	if ((window.document.form1.quitados.checked == false)&&(window.document.form1.alienados.checked == false))
	{
		error_string += "Favor informar se prefere buscar veículos quitados e/ou alienados.\n";
		PrimeiroErro = "quitados";
	}
	if (error_string == "")
	{
		return true;
	}
	else
	{
		alert(error_string);
		RecebeErro = 'document.form1.'+PrimeiroErro+'.focus();';
		eval(RecebeErro);
		return false;
	}	
}
</script> 
